  <script type="25e60ad60a0e2a63e48a1bcc-text/javascript">jQuery(document).ready(function() {
	jQuery( '.touchcarousel' ).on( "click", ".arrow-holder", function() {
		jQuery(this).addClass('qaz');
		jQuery(this).parent().find('.lazy-load').each(function(index, element){
			var thumb_url = jQuery(element).data('bg');
			if(thumb_url) {
				jQuery(element).css('background-image', "url('" + thumb_url + "')")
	                .removeAttr('data-bg')
	                .triggerHandler('load');
			}
		});
	});
});</script> 
