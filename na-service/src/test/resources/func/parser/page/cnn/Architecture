				rval = "election center:save races:" + CNN.saved_races;
			} else {
				if(wminst.Util.getCNNCapContentType()) {
					rval = "D=c13";
				}
			}
			return rval;
		},
        getCNNExploreIndentify: function(data) { //eVar79 for Politics Explorer
