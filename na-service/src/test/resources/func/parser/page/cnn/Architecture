function checkUserAuthentication() {
  var cnnauth = "";
  if (window.is_expansion) {
    // Do Nothing
  } else {
    if (typeof s.prop32 != 'undefined' && s.prop32 == "adbp:interactive") {
      cnnauth = wminst.Util.getCNNAuthenticated("authid", "displayname", "reg:logged in", "requires authentication", "reg:not logged in", "?");
    } else if (typeof s.prop28 != 'undefined' && s.prop28 == "watch cnn:activation") {
      cnnauth = wminst.Util.getCNNAuthenticated("authid", "displayname", "reg:logged in", "does not require authentication", "reg:not logged in", "?");
    } else {
      cnnauth = wminst.Util.getCNNAuthenticated("authid", "displayname", "reg:logged in", "anonymous", "reg:not logged in", "?");
    }
  }
  return cnnauth;
}

