  s.linkTrackVars = "events,server,pageName,channel,prop2,eVar2,prop4,evar4,prop8,eVar8,eVar15,prop16,eVar16,eVar26,eVar27,prop28,eVar28,prop32,eVar32,prop33,eVar33,prop35,eVar35,prop44,eVar44,prop46,eVar46,prop47,eVar47,eVar51,prop56,eVar56,prop57,eVar57,prop59,eVar59,prop64,eVar64,prop69,eVar69,prop73,eVar73,prop74,eVar74,prop75,eVar75,eVar89,list2";
