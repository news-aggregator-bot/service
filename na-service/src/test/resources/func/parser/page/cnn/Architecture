/*CNN Video SeventyFive */
PubSub.subscribe("cnnvideo-seventyfive", function(data) {
    wminst.isAd = false;
    var v = wminst.Util.getVideoMetadata(data);
    if (wminst.isMoneyVideo(v)) return;
    var vc = new wminst.getCNNMediaCollection();
    if (!vc.get(v.playerid, "isSeventyFive") && !vc.get(v.playerid, "hasScrubbed")) {
        vc.set(v.playerid, "isSeventyFive", true);
        var timeSpent = vc.progress(v.playerid);
        wminst.videoCommonFunction.videoCommonData(v);
        s.linkTrackVars = "events,server,channel,products,eVar1,eVar3,eVar4,eVar7,eVar8,eVar10,eVar11,eVar14,eVar16,eVar20,eVar22,prop23,eVar23,eVar26,eVar27,eVar28,eVar41,eVar29,eVar30,prop31,eVar31,eVar32,eVar33,eVar34,prop35,eVar35,eVar36,eVar37,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop50,eVar52,eVar53,eVar54,prop56,eVar56,eVar57,prop59,eVar59,eVar60,eVar64,eVar66,eVar67,eVar68,eVar70,eVar71,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
        s.linkTrackEvents = "event42";
        s.events = "event42";
        s.products = "";
        timeSpent = wminst.capCNNTimeSpent(timeSpent, v.trt, wminst.live_interval);
        if (timeSpent > 0) {
            s.linkTrackEvents = "event42,event36";
            s.events = "event42,event36";
            s.products = ";;;;" + "event36" + "=" + timeSpent;
        }
		if(wminst.is_autoplay === true){
			if (typeof s.prop70 == "string" && s.prop70 !== ""){
				s.prop70 = s.prop70.replace("noautostart", "autostart")
			}
		}
        s.eVar1 = s.prop1;
        s.prop1 = "";
        s.eVar3 = s.prop3;
        s.prop3 = "";
        s.eVar4 = s.prop4;
        s.prop4 = "";
        s.eVar7 = s.prop7;
        s.prop7 = "";

        if (s.prop8) {
            s.eVar8 = wminst.Util.getCNNVisitNumber(28);
            s.prop8 = "";
        }
        s.eVar10 = s.prop10;
        s.prop10 = "";
        s.eVar11 = s.prop11;
        s.prop11 = "";
        s.eVar14 = s.prop14;
        s.prop14 = "";
        s.eVar16 = s.prop16;
        s.prop16 = "";
        s.eVar20 = s.prop20;
        s.prop20 = "";
        s.eVar23 = s.prop23;
        s.prop23 = "";
        if (s.pageName) {
            s.eVar26 = s.pageName;
            s.pageName = "";
        } else {
            s.eVar26 = wminst.Util.getCNNPageName();
        }
        s.eVar27 = s.channel;
        s.channel = "";
        if (s.prop28) {
            s.eVar28 = wminst.Util.getCNNSection(1);
            s.prop28 = "";
        }
        if (s.server) {
            s.eVar29 = s.server;
            s.server = "";
        } else {
            s.eVar29 = wminst.Util.getADBPURL("domain");
        }
        s.eVar30 = s.prop30;
        s.prop30 = "";
        s.eVar41 = s.prop29;
        if (s.prop31) {
            s.eVar31 = s.prop31.toLowerCase();
            s.prop31 = "";
        } else {
            s.eVar31 = "";
        }
        s.eVar32 = s.prop32;
        s.prop32 = "";
        if (s.prop33) {
            s.eVar33 = s.prop33;
            s.prop33 = "";
        }
        s.eVar34 = s.prop34;
        s.prop34 = "";
        if (!s.prop35) {
            s.prop35 = wminst.Util.getCNNCodeVersion();
        }
        s.eVar37 = s.prop37;
        s.prop37 = "";
     		if(!s.prop43){ s.prop50 = ""; }
        s.eVar47 = "D=c47";
        s.eVar52 = s.prop52;
        s.prop52 = "";
        s.eVar53 = s.prop53;
        s.prop53 = "";
        s.eVar54 = s.prop54;
        s.prop54 = "";
		    s.eVar56 = s.prop56;
        s.prop56 = "";
        if (s.prop57) {
            s.eVar57 = s.prop57;
            s.prop57 = "";
        } 
        s.eVar60 = s.prop60;
        s.prop60 = "";
        s.eVar64 = wminst.Util.getCNNUIEngagement();
        s.prop64 = "";
        s.eVar66 = s.prop66;
        s.prop66 = "";
        s.eVar68 = s.prop68;
        s.prop68 = "";
		    s.eVar70 = s.prop70;
        s.prop70 = "";
        s.eVar71 = s.prop71;
        s.prop71 = "";
        s.eVar72 = s.prop72;
        s.prop72 = "";
        if (wminst.isAMPVideos) {
            s.timestamp = wminst.getAMPVideoTimeStamp();
        }
        wminst.trackLinkVideo("cnnvideo-seventyfive", v);
        s.manageVars("clearVars");
    }
});

/*CNN Video Complete */
PubSub.subscribe("cnnvideo-complete", function(data) {
    wminst.isAd = false;
    var v = wminst.Util.getVideoMetadata(data);
    var vc = new wminst.getCNNMediaCollection();
    var timeSpent = vc.complete(v.playerid);
    wminst.videoCommonFunction.videoCommonData(v);
    s.linkTrackVars = "events,server,channel,products,eVar1,eVar3,eVar4,eVar7,eVar8,eVar10,eVar11,eVar14,eVar16,eVar20,eVar22,prop23,eVar23,eVar26,eVar27,eVar28,eVar41,eVar29,eVar30,prop31,eVar31,eVar32,eVar33,eVar34,prop35,eVar35,eVar36,eVar37,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop50,eVar52,eVar53,eVar54,prop56,eVar56,eVar57,prop59,eVar59,eVar60,eVar64,eVar66,eVar67,eVar68,eVar70,eVar71,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    s.linkTrackEvents = "event33";
    s.events = "event33";
    timeSpent = wminst.capCNNTimeSpent(timeSpent, v.trt, wminst.live_interval);
    s.products = "";
    if (timeSpent > 0) {
        s.linkTrackEvents = "event33,event36";
        s.events = "event33,event36";
        s.products = ";;;;" + "event36" + "=" + timeSpent;
    }
	if (wminst.is_autoplay === true){
		if (typeof s.prop70 == "string" && s.prop70 !== ""){
			s.prop70 = s.prop70.replace("noautostart", "autostart")
		}
	}
    s.eVar1 = s.prop1;
    s.prop1 = "";
    s.eVar3 = s.prop3;
    s.prop3 = "";
    s.eVar4 = s.prop4;
    s.prop4 = "";
    s.eVar7 = s.prop7;
    s.prop7 = "";
    s.eVar8 = wminst.Util.getCNNVisitNumber(28);
    s.prop8 = "";
    s.eVar10 = s.prop10;
    s.prop10 = "";
    s.eVar11 = s.prop11;
    s.prop11 = "";
    s.eVar16 = s.prop16;
    s.prop16 = "";
    s.eVar20 = s.prop20;
    s.prop20 = "";
    s.eVar23 = s.prop23;
    s.prop23 = "";
    if (s.pageName) {
        s.eVar26 = s.pageName;
        s.pageName = "";
    } else {
        s.eVar26 = wminst.Util.getCNNPageName();
    }
    s.eVar27 = s.channel;
    s.channel = "";
    if (s.prop28) {
        s.eVar28 = wminst.Util.getCNNSection(1);
        s.prop28 = "";
    }
    if (s.server) {
        s.eVar29 = s.server;
        s.server = "";
    } else {
        s.eVar29 = wminst.Util.getADBPURL("domain");
    }
    s.eVar30 = s.prop30;
    s.prop30 = "";
    s.eVar41 = s.prop29;
    if (s.prop31) {
        s.eVar31 = s.prop31.toLowerCase();
        s.prop31 = "";
    } else {
        s.eVar31 = "";
    }
    if (s.prop32) {
        s.eVar32 = s.prop32;
        s.prop32 = "";
    }
    if (s.prop33) {
        s.eVar33 = s.prop33;
        s.prop33 = "";
    }
    s.eVar34 = s.prop34;
    s.prop34 = "";
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
    s.eVar37 = s.prop37;
    s.prop37 = "";
 		if(!s.prop43){ s.prop50 = ""; }
    s.eVar47 = "D=c47";
    s.eVar52 = s.prop52;
    s.prop52 = "";
    s.eVar53 = s.prop53;
    s.prop53 = "";
    s.eVar54 = s.prop54;
    s.prop54 = "";
	  s.eVar56 = s.prop56;
    s.prop56 = "";
    if (s.prop57) {
        s.eVar57 = s.prop57;
        s.prop57 = "";
    } 
    s.eVar60 = s.prop60;
    s.prop60 = "";
    s.eVar64 = wminst.Util.getCNNUIEngagement();
    s.prop64 = "";
    s.eVar66 = s.prop66;
    s.prop66 = "";
    s.eVar68 = s.prop68;
    s.prop68 = "";
	  s.eVar70 = s.prop70;
    s.prop70 = "";
    s.eVar71 = s.prop71;
    s.prop71 = "";
    s.eVar72 = s.prop72;
    s.prop72 = "";

    if (typeof v.isLive !== 'undefined' && (v.isLive == true || v.isLive == "true")) { //time limit value set by webdev
        if (wminst.setinterval_id) {
            clearInterval(wminst.setinterval_id);
        } //stop any progress calls
    }
    if (wminst.isAMPVideos) {
        s.timestamp = wminst.getAMPVideoTimeStamp();
    }
    if (wminst.isMoneyVideo(v)) {
        wminst.trackMoneyVideoEvent("complete", v);
    } else {
        s.eVar14 = s.prop14;
        s.prop14 = "";
        wminst.trackLinkVideo("cnnvideo-complete", v);
    }
    vc.set(v.playerid, "vidStarted", false);
    vc.set(v.playerid, "hasScrubbed", false);
    vc.set(v.playerid, "isPaused", false);
    vc.set(v.playerid, "isBuffering", false);
    vc.set(v.playerid, "isHalf", false);
    vc.set(v.playerid, "isTen", false);
    vc.set(v.playerid, "isTwentyFive", false);
    vc.set(v.playerid, "isSeventyFive", false);
    s.manageVars("clearVars");
});
wminst.videoCommonFunction = function() {
    return {
        videoCommonData: function(data) {
            s.manageVars("clearVars");
            wminst.standardVideoMetadata.isCNNGo = false;
            var isVideoAutoStarted = wminst.Util.isVideoAutoStarted(data);
            if (typeof data.free_preview !== "undefined" && data.free_preview !== "") {
                wminst.standardVideoMetadata.isCNNGo = true;
            }
            if (data.video_player && data.video_player.indexOf("theo") !== -1) {
                if (data.isVr === true) {
                    s.prop52 = "theo360";
                    s.eVar52 = "D=c52";
                    s.prop33 = "adbp:video:360";
                    s.eVar33 = "D=c33";
                } else {
                    s.prop52 = data.video_player;
                    s.eVar52 = "D=c52";
                    s.prop33 = "adbp:video start";
                    s.eVar33 = "D=c33";
                    if (typeof window.CNN.omniture !== 'undefined') {
                        try {
                            s.prop14 = CNN.omniture.branding_social || "";
                            s.eVar14 = "D=c14";
                        } catch (e) {}
                    }
                }
            }
            s.prop32 = wminst.Util.getCNNTemplateType("long");
            if (s.prop32 && s.prop32 != "") {
                s.eVar32 = "D=c32";
            }
            s.prop37 = wminst.Util.getCNNPlatform();
            if (s.prop37 && s.prop37 != "") {
                s.eVar37 = "D=c37";
            }
         		if(typeof data.id != 'undefined' && data.id != "" && data.id.indexOf('invalid-id-video-player') != -1) {
             	s.prop43 = wminst.Util.getCNNPostID();
            	s.prop50 = wminst.Util.getCNNPostTitle();
            }
            s.list1 = wminst.Util.getCEPTopisForVideo(data) || wminst.Util.getCNNTopic();
            s.prop72 = data.free_preview || "";
            s.eVar72 = "D=c72";
            //s.prop59 = wminst.Util.getCNNAdobeID();
            //s.eVar59 = "D=c59";
            if (typeof data.video_focus !== 'undefined') {
                s.prop66 = data.video_focus;
                s.eVar66 = "D=c66";
            }
            try {
                wminst.standardVideoMetadata.player = CNN.omniture.video_player_type;
            } catch (e) {
                wminst.standardVideoMetadata.player = "";
            }
            if (typeof data.isLive !== 'undefined' && (data.isLive == "true" || data.isLive == true)) {
                wminst.standardVideoMetadata.player = "live player";
                s.prop53 = data.live_stream_name ? data.live_stream_name : "cnn news";
                s.eVar53 = "D=c53";
            }
            if (!wminst.standardVideoMetadata.player) {
                wminst.standardVideoMetadata.player = "vod player";
            } //prop1,eVar1
            if (data.player_type && data.player_type != "") {
                wminst.standardVideoMetadata.player = data.player_type;
                if (data.player_type == "live") {
                    wminst.standardVideoMetadata.player = "live player";
                }
                if (data.player_type == "van") {
                    s.prop31 = "van";
                    s.eVar31 = "D=c31";
                    s.prop32 = "adbp:video";
                    s.eVar32 = "D=c32";
                    s.server = s.eVar29 = data.consumer || "";
                    s.prop71 = data.source.toLowerCase() || "";
                    s.eVar71 = "D=c71";
                } else if (data.player_type == "tve" || data.player_type == "live" || data.player_type == "live player") {
                    s.prop3 = data.category || "";
                    s.eVar3 = "D=c3";
                    s.prop31 = "van";
                    s.eVar31 = "D=c31";
                    s.prop53 = data.live_stream_name ? data.live_stream_name : "cnn news";
                    s.eVar53 = "D=c53";
                    /*if (data.id == "cvplive/cnngo") {
                        s.prop57 = (typeof data.mvpd !== "undefined" && typeof data.mvpd == "string")? data.mvpd.toLowerCase(): wminst.Util.getCNNMVPD();
                        s.eVar57 = "D=c57";
                    } //prop57,eVar57
                    s.prop59 = data.adobe_hash_id ? data.adobe_hash_id : wminst.standardVideoMetadata.adobe_hash_id;
                    s.eVar59 = "D=c59";*/
                }
            }
            var player = "";
            try {
                if (typeof window.CNN.omniture.video_player_type !== 'undefined' && window.CNN.omniture.video_player_type != '') {
                    player = window.CNN.omniture.video_player_type;
                } else if (typeof data.isLive !== "undefined" && (data.isLive == "true" || data.isLive == true)) {
                    player = "live player";
                } else if (typeof data.player_type !== 'undefined' && data.player_type != "") {
                    player = data.player_type;
                    if (player == "live") {
                        player = "live player";
                    }
                }

            } catch (e) {
                player = "";
            }
            if (!player) { //default
                player = "vod player";
            }
            s.prop1 = player;
            s.eVar1 = "D=c1";
            try {
                if ((window.location.href.indexOf("fave.api.cnn.io") > -1 || window.location.href.indexOf("fave-api.cnn.com") > -1) && (data.video_player && data.video_player.indexOf("theo") !== -1)) {
                    s.prop32 = "content:video:nocollection";
                    if (typeof window.CNN.omniture.template_type !== 'undefined' && window.CNN.omniture.template_type == "live-story") {
                        s.prop32 = "other:live story";
                    }
                    s.eVar32 = "D=c32";
                }
            } catch (e) {}
            try {
                s.prop2 = window.CNN.omniture.cap_author || "";
                s.eVar2 = "D=c2";
            } catch (e) {} //prop2,eVar2
            try {
                s.prop3 = data.id.split("/")[0];
                s.eVar3 = "D=c3";
            } catch (e) {} //prop3,eVar3
            try {
                s.prop7 = data.trt ? data.trt : "";
                s.eVar7 = "D=c7";
            } catch (e) {} //prop7,eVar7
            s.prop8 = wminst.Util.getCNNVisitNumber(28);
            var d = (data.dateCreated && data.dateCreated.text ? data.dateCreated.text : "");
            var p = (d != "" ? d.split("/") : "");
            if (p != "" && p[0].length != 4) {
                d = "20" + p[2] + "/" + p[0] + "/" + p[1];
            }
            var days = wminst.Util.getCNNDaysSinceLastPublish(d);
            var md = (data.lastModified && data.lastModified.text ? data.lastModified.text : "");
            var updated_date = d + "|" + md;
            s.prop10 = days + "";
            s.eVar10 = "D=c10";
            s.prop16 = updated_date;
            s.eVar16 = "D=c16";
            try {
                if (typeof data.subcategory !== 'undefined' && data.subcategory == 'default') {
                    if (typeof window.CNN.omniture.branding_content_page != 'undefined') {
                        s.prop11 = "v:" + window.CNN.omniture.branding_content_page;
                        s.eVar11 = "D=c11";
                    }
                } else if (typeof data.sponsor !== 'undefined' && data.sponsor) {
                    s.prop11 = "v:" + data.sponsor;
                    s.eVar11 = "D=c11";
                } else if (data.subcategory && typeof data.subcategory !== "undefined" && data.subcategory != ""){
                    s.prop11 = "v:" + data.subcategory;
                    s.eVar11 = "D=c11";
                }
            } catch (e) {}

            var hostName = window.location.hostname;
            if (hostName.indexOf("fave.api.cnn.io") != -1 || hostName.indexOf("fave-api.cnn.com") != -1) {
                var pathName = window.location.pathname;
                if (pathName.indexOf("/v1/amp") != -1) {
                    s.prop14 = "google amp";
                    s.eVar1 = "D=c14";
                }
                if (pathName.indexOf("/v1/fav") != -1) {
                    s.prop14 = "embed";
                    s.eVar1 = "D=c14";
                }
                if (pathName.indexOf("/v1/synacor") != -1) {
                    s.prop14 = "synacor player";
                    s.eVar1 = "D=c14";
                    s.prop1 = "synacor player";
                    s.eVar1 = "D=c1";
                }
            }
            try {
                if (typeof window.CNN.omniture !== 'undefined' && window.CNN.omniture.fbia === true) {
                    s.prop14 = window.CNN.omniture.branding_social;
                    s.eVar1 = "D=c14";
                    var page_name = CNN.omniture.vpage_name;
                    s.pageName = wminst.Util.getCNNPageName(page_name);
                    s.eVar26 = "D=pageName";
                }
            } catch (e) {}
            s.eVar22 = wminst.Util.getCNNVideoOpportunity(); //eVar22
            s.server = wminst.Util.getADBPURL("domain"); // server 
            s.channel = wminst.Util.getCNNSection(0); //channel   
            s.prop28 = wminst.Util.getCNNSection(1); //subsection
            s.pageName = wminst.Util.getCNNPageName(); //pagename 
            wminst.isGoogleAmp = s.prop14;
            try {
                if (!data.title && typeof data.headline !== 'undefined') {
                    data.title = data.headline.toLowerCase(); // assign data.headline if data.title is not available
                }
                if (wminst.standardVideoMetadata.player == "van" && (typeof data.title != 'undefined' || data.title != "" || data.title == "")) {
                    s.prop29 = "/video/" + data.title;
                    s.eVar41 = "D=c29";
                } else if (wminst.standardVideoMetadata.player == "vod player" && (typeof data.category != 'undefined' || data.category != "") && data.category == "vr") {
                    s.prop29 = data.title;
                    s.eVar41 = "D=c29";
                } else {
                    s.prop29 = "/video/" + data.id;
                    s.eVar41 = "D=c29";
                }
            } catch (e) {}
            s.prop30 = wminst.Util.getSiteSpecificSettings(1);
            s.eVar30 = "D=c30";
            try {
                s.prop31 = window.CNN.omniture.cap_show_name;
                s.eVar31 = "D=c31";
            } catch (e) {} //prop31,eVar31

            s.prop34 = "does not require authentication";
            s.eVar34 = "D=c34";
            try {
                if (data.auth_state) {
                    s.prop34 = data.auth_state;
                    s.eVar34 = "D=c34";
                } else if (typeof data.mvpd !== "undefined" && (data.mvpd == "temppass_cnn10min" || data.mvpd == "TempPass_CNN10min")) {
                    s.prop34 = "requires authentication";
                    s.eVar34 = "D=c34";
                } else if (typeof data.mvpd !== "undefined" && typeof data.mvpd == "string" && data.mvpd !== "") {
                    s.prop34 = "requires authentication";
                    s.eVar34 = "D=c34";
                }
            } catch (e) {}

            if (!s.eVar36) {
                s.eVar36 = wminst.Util.getCNNKruxID();
            }
            if (wminst.standardVideoMetadata.player == "vod player" && (typeof data.category != 'undefined' || data.category != "") && data.category == "vr") {
                s.eVar42 = data.id;
            } else {
                s.eVar42 = "/video/" + data.id;
            }
            if (!s.prop44) {
                s.prop44 = wminst.Util.getCNNSourceID();
            }
            s.prop46 = wminst.Util.getCNNTransactionID();
            var vc_type1 = "vod",
                vc_type2 = "non tve",
                vc_type3 = "clip",
                vc_type4 = "clip";
            if (wminst.standardVideoMetadata.player.indexOf("live") != -1 && s.prop34 == "requires authentication") {
                vc_type1 = "live";
                vc_type2 = "tve";
                vc_type3 = "live";
                vc_type4 = "live";
            }
            if (wminst.standardVideoMetadata.player.indexOf("live") != -1 && s.prop34 == "does not require authentication") {
                vc_type1 = "live";
                vc_type2 = "non tve";
                vc_type3 = "live";
                vc_type4 = "live";
            }
            if (wminst.standardVideoMetadata.player.indexOf("tve") != -1) {
                vc_type1 = "live";
                vc_type2 = "tve";
                vc_type3 = "live";
                vc_type4 = "live";
            }
            var vc_type5 = wminst.isAd ? "ad" : "content";
            s.prop54 = "video:" + vc_type1 + ":" + vc_type2 + ":" + vc_type3 + ":" + vc_type4 + ":" + vc_type5;
            s.eVar54 = "D=c54";
			s.prop56 = wminst.Util.getCNNOrientation();
            /*s.prop57 = (typeof data.mvpd !== "undefined" && typeof data.mvpd == "string")? data.mvpd.toLowerCase(): wminst.Util.getCNNMVPD();*/
            try {
                if (data.dateAired) {
                    var days2 = wminst.Util.gCNNDaysSinceLastPublish(data.dateAired);
                    s.prop58 = days2 + "";
                    s.eVar58 = "D=c58";
                }
            } catch (e) {}
            try {
                if (window.CNN && window.CNN.omniture && window.CNN.omniture.cap_content_type == "article") {
                    if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                        if (isVideoAutoStarted === true) {
                            s.prop70 = "autostart:editorial";
                        } else {
                            s.prop70 = "noautostart:editorial";
                        }
                    } else {
                        s.prop70 = "noautostart:editorial";
                    }
                    s.eVar70 = "D=c70";
                } else if (data.isLive && (data.isLive == true || data.isLive == "true")) {
                    if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                        if (isVideoAutoStarted === true) {
                            s.prop70 = "autostart:live";
                        } else {
                            s.prop70 = "noautostart:live";
                        }
                    } else {
                        s.prop70 = "noautostart:live";
                    }
                    s.eVar70 = "D=c70";
                } else {
                    if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                        if (isVideoAutoStarted === true) {
                            s.prop70 = "autostart:vod";
                        } else {
                            s.prop70 = "noautostart:vod";
                        }
                    } else {
                        s.prop70 = "noautostart:vod";
                    }
                    s.eVar70 = "D=c70";
                }
            } catch (e) {}
            try {
                if (window.location.pathname.match(/video\/playlists\/./)) {
                    var collection = /video\/playlists\/(.+)/.exec(window.location.pathname);
                    s.prop60 = collection[1].replace(/\//g, "");
                    s.eVar60 = "D=c60";
                    if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                        if (isVideoAutoStarted === true) {
                            s.prop70 = "autostart:collection";
                        } else {
                            s.prop70 = "noautostart:collection";
                        }
                    } else {
                        s.prop70 = "noautostart:collection";
                    }
                    s.eVar70 = "D=c70";
                }
                if (data.playerid != "player-one-tap-video") {
                    if (window.CNN.omniture.video_collection) {
                        s.prop60 = window.CNN.omniture.video_collection;
                        s.eVar60 = "D=c60";
                        if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                            if (isVideoAutoStarted === true) {
                                s.prop70 = "autostart:collection";
                            } else {
                                s.prop70 = "noautostart:collection";
                            }
                        } else {
                            s.prop70 = "noautostart:collection";
                        }
                        s.eVar70 = "D=c70";
                    }
                    if (window.video_collection) {
                        if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                            if (isVideoAutoStarted === true) {
                                s.prop70 = "autostart:collection:sunrise";
                            } else {
                                s.prop70 = "noautostart:collection:sunrise";
                            }
                        } else {
                            s.prop70 = "autostart:collection:sunrise";
                        }
                        s.eVar70 = "D=c70";
                    }
                }
                if (data.video_collection) {
                    s.prop60 = data.video_collection;
                    s.eVar60 = "D\x3dc60";
                    if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                        if (isVideoAutoStarted === true)
                            s.prop70 = "autostart:collection";
                        else
                            s.prop70 = "noautostart:collection";
                    } else {
                        s.prop70 = "noautostart:collection";
                        s.eVar70 = "D\x3dc70";
                    }
                }
                if (s.prop60 == "" || typeof s.prop60 == "undefined") {
                    if (window.CNN.contentModel.analytics.pageTop.collectionHeadline) {
                        s.prop60 = window.CNN.contentModel.analytics.pageTop.collectionHeadline;
                        s.eVar60 = "D=c60";
                    }
                }
                if (s.prop60 !== "") {
                    s.prop60 = s.prop60.toLowerCase();
                }
            } catch (e) {}
            try {
                if (data.iscmsIIimport) {
                    var airSynd = (data.iscmsIIimport == "true") ? "secondary" : "primary";
                    s.prop61 = airSynd;
                    s.eVar61 = "D=c61";
                }
                if (data.source) {
                    s.prop71 = data.source.toLowerCase();
                    s.eVar71 = "D=c71";
                }
            } catch (e) {}
            try {
                if (data.ad_duration) {
                    wminst.adRange = Math.round(data.ad_duration).toString();
                    s.prop68 = wminst.adRange;
                    s.eVar68 = "D=c68";
                } else {
                    s.prop68 = "no ad present";
                    s.eVar68 = "D=c68";
                }
            } catch (e) {}
            try {
                if (typeof data.id !== "undefined" && data.id.indexOf("cvplive/cnngo") != -1) {
                    s.prop64 = "ngtv";
                }
            } catch (e) {}

            if (data.content_type2) { //ngtv player
                s.prop1 = "ngtv";
                s.eVar1 = "D=c1";
                s.prop2 = data.author;
                s.eVar2 = "D=c2";
                if (data.content_type2 == "live" || data.content_type2 == "dvr") {
                    s.prop7 = s.eVar7 = "";
                    var livename = "cnn news";
                    if (data.live_stream_name) {
                        livename = data.live_stream_name;
                    }
                    s.prop53 = livename;
                    s.eVar53 = "D=c53";
                }
                try {
                    if (data.dateCreated) {
                        var days1 = wminst.Util.gCNNDaysSinceLastPublish(data.dateCreated);
                        s.prop10 = days1 + "";
                        s.eVar10 = "D=c10";
                        s.prop16 = data.dateCreated;
                        s.eVar16 = "D=c16";
                    }
                } catch (e) {}
                s.prop11 = s.eVar11 = "";
                s.prop14 = s.eVar14 = "";
                data.show_name = data.show_name || "";
                data.episode_title = data.episode_title || "";
                data.segment_name = data.segment_name || "";
                if (data.content_type1 == "tve") { //live, dvr, featured videos
                    data.title = data.show_name + ":" + data.episode_title + ":" + data.segment_name;
                    if (data.live_stream_name && data.live_stream_name == "hln news") {
                        data.title = "hln news";
                    }
                    s.prop29 = data.title;
                    s.prop32 = "adbp:video";
                    s.eVar33 = "D=c33";
                    s.eVar33 = "D=pageName";
                }
                s.prop31 = data.show_name;
                s.eVar31 = "D=c31";
                s.prop33 = "adbp:video start";
                s.eVar33 = "D=c33";
                s.eVar42 = data.id;
                if (typeof data.isBreakingNews != "undefined" && data.isBreakingNews == true) {
                    s.eVar43 = "ngtv:breaking_news";
                }
                if (data.segment_type) {
                    s.eVar43 = data.segment_type;
                } //eVar43
                data.content_type1 = data.content_type1 || "";
                data.content_type2 = data.content_type2 || "";
                data.content_type3 = data.content_type3 || "";
                data.content_type4 = data.content_type4 || "";
                data.content_type5 = wminst.isAd ? "ad" : "content";
                s.prop54 = "video:" + v.content_type2 + ":" + v.content_type1 + ":" + v.content_type3 + ":" + v.content_type4 + ":" + v.content_type5;
                /*s.prop57 = (typeof data.mvpd !== "undefined" && typeof data.mvpd == "string")? data.mvpd.toLowerCase(): wminst.Util.getCNNMVPD();
                s.eVar57 = "D=c57";*/
                if (data.content_type3 && data.content_type3 != "episode") { //live video
                    s.prop58 = s.eVar58 = "";
                }
                /*s.prop59 = data.adobe_hash_id;
                s.eVar59 = "D=c59";*/
                s.prop60 = s.eVar60 = "";
                s.prop61 = s.eVar61 = "";
                s.prop64 = s.eVar64 = "";
                if (data.content_type1 == "non tve" && data.content_type2 == "vod" && data.content_type3 == "clip") { //extended content videos
                } else {
                    s.prop68 = s.eVar68 = "";
                    s.prop71 = s.eVar71 = "";
                }
                if (typeof data.isAutoStart !== "undefined" && data.isAutoStart == true) {
                    if (isVideoAutoStarted === true) {
                        s.prop70 = "autostart:cnngo";
                    } else {
                        s.prop70 = "noautostart:cnngo";
                    }
                } else {
                    s.prop70 = "noautostart:cnngo";
                }
                s.eVar70 = "D=c70";
            }
            if (data.headline) {
                s.prop23 = data.headline.toLowerCase();
                s.eVar23 = "D=c23";
            }
            s.list1 = wminst.Util.getCEPTopisForVideo(data) || wminst.Util.getCNNTopic();
            s.eVar67 = wminst.Util.getCNNPlayerState(data); //eVar67
            try {
                window.CNN.omniture.gallery_name = "";
            } catch (err) {} //suppress c6/v6
            try {
                if (window.CNN && window.CNN.omniture != 'undefined' && window.CNN.omniture.is_vision == true) {
                    s.prop71 = "cnn:vision";
                    s.eVar71 = "D=c71";
                }
            } catch (e) {}
            try {
                s.prop4 = wminst.Util.getPageAttribution();
                s.prop46 = wminst.Util.getCNNTransactionID();
                s.prop47 = wminst.Util.getCookie("ug");
                s.prop26 = wminst.Util.getCNNBaseURL();
                s.prop75 = wminst.Util.getCNNUserAuthState();
            } catch (e) {}
			s.prop57 = (typeof data.mvpd !== "undefined" && typeof data.mvpd == "string" && data.mvpd !==  "")? data.mvpd.toLowerCase(): wminst.Util.getCNNMVPD();
            s.eVar57 = "D=c57";
			if(typeof data.adobe_hash_id !== "undefined" && typeof data.adobe_hash_id !== ""){
				s.prop59 = data.adobe_hash_id;
				s.eVar59 = "D=c59";
			}else if (typeof data.adobeHashId !== "undefined" && data.adobeHashId !== ""){
				s.prop59 = data.adobeHashId;
				s.eVar59 = "D=c59";
			}else{
				s.prop59 = wminst.Util.getCNNAdobeID();
				s.eVar59 = "D=c59";
			}
            //return standardVideoMetadata;
        }
    };
}();

});</script>
