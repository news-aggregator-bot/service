PubSub.subscribe("cnnvideo-preroll", function(data) {
    wminst.isAd = true;
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    wminst.adVidId = v.id;
    s.linkTrackVars = "events,server,channel,eVar1,eVar3,eVar4,eVar7,eVar8,eVar10,eVar11,eVar14,eVar16,eVar20,eVar22,eVar26,eVar27,eVar28,eVar29,eVar30,prop31,eVar31,eVar32,eVar33,eVar34,prop35,eVar35,eVar36,eVar37,eVar41,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop49,eVar49,prop50,eVar52,eVar53,eVar54,eVar56,eVar57,prop59,eVar59,eVar60,eVar64,eVar66,eVar67,eVar68,eVar70,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    s.linkTrackEvents = "event35";
    s.events = "event35";
    if (typeof v.adType == "string" && v.adType.toLowerCase() == "midroll") {
		s.linkTrackEvents = "event25";
		s.events = "event25";
    }
    if (typeof v.adType == "string" && v.adType.toLowerCase() == "preroll") {
        s.linkTrackEvents = s.linkTrackEvents + ",event90";
        s.events = s.linkTrackEvents;
        wminst.is_preroll_occur = true;
    }
    if (v.category && v.category !== "live" || v.isLive !== "true" || v.isLive !== true) {
        wminst.isLiveVid = false;
    }
    if (wminst.standardVideoMetadata.isCNNGo) {
        s.eVar72 = v.free_preview;
    } else {
        s.eVar72 = "";
    }
	if((typeof v.is_autoplay_allowed !== "undefined" && v.is_autoplay_allowed === true) || (typeof v.isAutoStart !== "undefined" && v.isAutoStart === true) ){
		wminst.is_autoplay = true;
		if (typeof s.prop70 == "string" && s.prop70 !== ""){
			s.prop70 = s.prop70.replace("noautostart", "autostart");
		}
	}
    s.eVar1 = s.prop1;
    s.prop1 = "";
    s.eVar3 = s.prop3;
    s.prop3 = "";
    if (s.prop4) {
        s.eVar4 = s.prop4;
        s.prop4 = "";
    } else {
        s.eVar4 = "";
    }
    s.eVar7 = s.prop7;
    s.prop7 = "";
    if (s.prop8) {
        s.eVar8 = s.prop8;
        s.prop8 = "";
    } else {
        s.eVar8 = wminst.Util.getCNNVisitNumber(28);
    }
    s.eVar10 = s.prop10;
    s.prop10 = "";
    s.eVar11 = s.prop11;
    s.prop11 = "";
    s.eVar16 = s.prop16;
    s.prop16 = "";
    s.eVar23 = s.prop23 = "";
    if (s.pageName) {
        s.eVar26 = s.pageName;
        s.pageName = "";
    } else {
        s.eVar26 = wminst.Util.getCNNPageName();
    }
    if (s.channel) {
        s.eVar27 = s.channel;
        s.channel = "";
    } else {
        s.eVar27 = wminst.Util.getCNNSection(0);
    }
    if (s.prop28) {
        s.eVar28 = s.prop28;
        s.prop28 = "";
    } else {
        s.eVar28 = wminst.Util.getCNNSection(1);
    }
    if (s.server) {
        s.eVar29 = s.server;
        s.server = "";
    } else {
        s.eVar29 = wminst.Util.getADBPURL("domain");
    }
    if (s.prop31) {
        s.eVar31 = s.prop31.toLowerCase();
        s.prop31 = "";
    } else {
        s.eVar31 = "";
    }
    s.prop30 = "";
    s.eVar32 = s.prop32;
    s.prop32 = "";
    s.eVar33 = s.prop33;
    s.prop33 = "";
    s.eVar34 = s.prop34;
    s.prop34 = "";
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
    s.eVar37 = s.prop37;
    s.prop37 = "";
    s.eVar41 = s.prop29;
    s.prop29 = "";
    s.eVar52 = s.prop52;
    s.prop52 = "";
    s.eVar54 = s.prop54;
    s.prop54 = "";
	s.eVar56 = s.prop56;
