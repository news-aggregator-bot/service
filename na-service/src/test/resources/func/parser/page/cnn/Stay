			"justice":        "CNNJUSTICE",
			"elections":      "CNNPOLITICS",
			"style":			    "CNNSTYLE"
		} [ch];
	if (pn) { c4 = pn; }
	if(pn && ch == "elections"){ c6 = "ELECTION";}
  if (wminst.isMoneyVideo(data)) {
     		c4 = "CNNBUSINESS";
     		c6 = "BUSINESS";
   }
    var cs_ucfr = wminst.Util.isTagConsented("comscore") ? "1" : "0";
