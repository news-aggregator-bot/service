        ns_st_pr: data.subcategory || "*null",
        ns_st_ep: data.headline || "*null",
        ns_st_sn: "*null",
        ns_st_en: "*null",
        ns_st_ge: data.category || "*null",
        ns_st_ia: "0",
        ns_st_ce: "0",
        ns_st_ddt: "*null",
        ns_st_tdt: adate || "*null",
        c3: c3,
        c4: c4,
        c6: c6,
        cs_ucfr: cs_ucfr
