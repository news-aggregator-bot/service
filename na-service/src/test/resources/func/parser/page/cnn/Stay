		    s.eVar70 = s.prop70;
        s.prop70 = "";
        s.eVar71 = s.prop71;
        s.prop71 = "";
        s.eVar72 = s.prop72;
        s.prop72 = "";
        if (wminst.isAMPVideos) {
            s.timestamp = wminst.getAMPVideoTimeStamp();
        }
        if (wminst.isMoneyVideo(v)) {
            wminst.trackMoneyVideoEvent("fifty", v);
        } else {
            s.eVar14 = s.prop14;
            s.prop14 = "";
            wminst.trackLinkVideo("cnnvideo-fifty", v);
        }
        s.manageVars("clearVars");
    }
});

