PubSub.subscribe("cnnvideo-adcomplete", function(data) {
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    s.linkTrackVars = "events,server,channel,prop35,eVar35,prop44";
    s.linkTrackEvents = "event50";
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
    if (v.adType.toLowerCase() == "midroll" || wminst.isLiveVid == true && v.adType.toLowerCase() == "preroll") {
        if (v.player_type && (v.player_type == "tve" || v.player_type == "live player" || v.isLive == "true")) {
            s.prop33 = "adbp:none";
            wminst.trackLinkVideo("cnnvideo-adcomplete", v);
        }
        if ((v.content_type1 && v.content_type1 == "tve") || (v.isLive === true || v.isLive == "true")) {
            if (wminst.setinterval_id)
                clearInterval(wminst.setinterval_id);
            wminst.setinterval_id = setInterval(function() {
                window.trackVideoProgress(v);
            }, 6E4);
        }
    }
    wminst.isAd = false;
    s.manageVars("clearVars");
});

/*CNN Video Midroll Complete*/
PubSub.subscribe("cnnvideo-midroll-complete", function(data) {
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    s.linkTrackVars = "events,server,channel,prop35,eVar35,prop44";
    s.linkTrackEvents = "event50";
    s.events = "event50";
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
    if ((v.adType.toLowerCase() == "midroll") || (wminst.isLiveVid == true && v.adType.toLowerCase() == "preroll")) {
        if (v.player_type && (v.player_type == "tve" || v.player_type == "live player" || v.isLive == "true" || v.isLive === true)) {
            s.prop33 = "adbp:none";
            wminst.trackLinkVideo("cnnvideo-adcomplete", v);
        }
    }
    if (wminst.setinterval_id) {
        clearInterval(wminst.setinterval_id);
    }
    wminst.setinterval_id = setInterval(function() {
        window.trackVideoProgress(v);
    }, 60000);
    wminst.isAd = false;
    s.manageVars("clearVars");
});

/*CNN Video Start*/
PubSub.subscribe("cnnvideo-start", function(data) {
    wminst.isAd = false;
    wminst.isLiveVid = false;
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    var isVideoAutoStarted = wminst.Util.isVideoAutoStarted(v);
    var eventType = "cnnvideo-start";
    s.linkTrackVars = "events,server,channel,prop1,eVar1,prop3,eVar3,prop4,eVar4,prop7,eVar7,prop8,eVar8,prop10,eVar10,prop11,eVar11,prop14,eVar14,prop16,eVar16,prop20,eVar20,eVar21,eVar22,eVar23,eVar26,eVar27,eVar28,prop29,eVar29,prop30,eVar30,prop31,eVar31,prop32,eVar32,prop33,eVar33,prop34,eVar34,prop35,eVar35,eVar36,prop37,eVar37,eVar41,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop49,eVar49,prop50,prop52,eVar52,prop54,eVar54,prop56,eVar56,prop57,eVar57,prop59,eVar59,prop60,eVar60,eVar61,prop64,eVar64,eVar54,prop66,eVar66,eVar67,prop68,eVar68,prop70,eVar70,prop71,eVar71,prop72,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    if (isVideoAutoStarted === true) {
        s.linkTrackEvents = "event32,event34";
        s.events = "event32,event34";
        eventType = "cnnvideo-autostart";
		wminst.is_autoplay = true;
        if (typeof s.prop70 == "string" && s.prop70 !== "")
            s.prop70 = s.prop70.replace("noautostart", "autostart");
    } else {
        s.linkTrackEvents = "event32";
        s.events = "event32";
    }
    if (typeof v.subcategory && v.subcategory && v.subcategory != "") {
        s.linkTrackEvents = "event23," + s.linkTrackEvents;
        s.events = s.linkTrackEvents;
    }
    if (typeof v.metas !== 'undefined' && v.metas.branding && v.metas.branding != "") {
        s.linkTrackEvents = "event22," + s.linkTrackEvents;
        s.events = s.linkTrackEvents;
    }
    if (wminst.adVidId == v.id && wminst.adRange > 0) {
        s.linkTrackEvents = "event3," + s.linkTrackEvents;
        s.events = s.linkTrackEvents;
        wminst.adVidId = "";
    }
    if (!wminst.is_preroll_occur) {
        s.linkTrackEvents = s.linkTrackEvents + ",event90";
        s.events = s.linkTrackEvents;
    }
    var vc = new wminst.getCNNMediaCollection();
    vc.start(v.playerid, v.title ? v.title : v.headline);
    if (wminst.standardVideoMetadata.isCNNGo) {
        s.prop72 = v.free_preview;
        s.eVar72 = "D\x3dc72";
    } else
        s.prop72 = s.eVar72 = "";
    if (s.prop4) {
        s.eVar4 = "D=c4";
    } else {
        s.eVar4 = "";
    }
    s.eVar21 = wminst.Util.getCNNVideoSequence();
    s.eVar23 = s.prop23;
    s.prop23 = "";
    if (s.prop28) {
        s.eVar28 = wminst.Util.getCNNSection(1);
        s.prop28 = "";
    }
    if (!s.prop31) {
        s.eVar31 = "";
    } else {
        s.prop31 = s.prop31.toLowerCase();
    }
    if (s.prop33) {
        s.eVar33 = "D=c33";
    }
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
 		if(!s.prop43){ s.prop50 = ""; }
    s.eVar47 = "D=c47";
    s.eVar60 = s.prop60;
    s.prop60 = "";
    s.prop64 = "";
    s.eVar64 = wminst.Util.getCNNUIEngagement();
    if (wminst.isAMPVideos) {
        s.timestamp = wminst.getAMPVideoTimeStamp();
    }
    if (wminst.isMoneyVideo(v))
        wminst.trackMoneyVideoEvent("start", v);
    else {
        wminst.trackLinkVideo(eventType, v);
        vc.set(v.playerid, "vidStarted", true);
    }
    s.manageVars("clearVars");
});

/*CNN Auto Start*/
PubSub.subscribe("cnnvideo-autostart", function(data) {
    wminst.isAd = false;
    wminst.isLiveVid = false;
    var eventType = "cnnvideo-autostart";
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    var isVideoAutoStarted = wminst.Util.isVideoAutoStarted(v);
    s.linkTrackVars = "events,server,channel,prop1,eVar1,prop3,eVar3,prop4,eVar4,prop7,eVar7,prop8,eVar8,prop10,eVar10,prop11,eVar11,prop14,eVar14,prop16,eVar16,prop20,eVar20,prop21,eVar21,eVar22,prop23,eVar23,eVar26,eVar27,eVar28,prop29,eVar29,prop30,eVar30,prop31,eVar31,prop32,eVar32,prop33,eVar33,prop34,eVar34,prop35,eVar35,eVar36,prop37,eVar37,eVar41,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop49,eVar49,prop50,prop52,eVar52,prop54,eVar54,prop56,eVar56,prop57,eVar57,prop59,eVar59,prop60,eVar60,eVar61,prop64,eVar64,prop66,eVar66,eVar67,prop68,eVar68,prop70,eVar70,prop71,eVar71,prop72,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    
	if (isVideoAutoStarted === false) {
        s.linkTrackEvents = "event32";
        s.events = "event32";
        eventType = "cnnvideo-start";
    } else {
        s.linkTrackEvents = "event32,event34";
        s.events = "event32,event34";
		wminst.is_autoplay = true;
        if (typeof s.prop70 == "string" && s.prop70 !== "")
            s.prop70 = s.prop70.replace("noautostart", "autostart");
    }
    if (typeof v.subcategory && v.subcategory && v.subcategory != "") {
        s.linkTrackEvents = "event23," + s.linkTrackEvents;
        s.events = s.linkTrackEvents;
    }
    if (typeof v.metas !== 'undefined' && v.metas.branding && v.metas.branding != "") {
        s.linkTrackEvents = "event22," + s.linkTrackEvents;
        s.events = s.linkTrackEvents;
    }
    if (wminst.adVidId == v.id && wminst.adRange > 0) {
        s.linkTrackEvents = "event3," + s.linkTrackEvents;
        s.events = s.linkTrackEvents;
        wminst.adVidId = "";
    }
    if (!wminst.is_preroll_occur) {
        s.linkTrackEvents = s.linkTrackEvents + ",event90";
        s.events = s.linkTrackEvents;
    }
    var vc = new wminst.getCNNMediaCollection();
    vc.start(v.playerid, v.title ? v.title : v.headline);
    if (s.prop4) {
        s.eVar4 = "D=c4";
    } else {
        s.eVar4 = "";
    }
    s.prop21 = "";
    s.eVar23 = data.headline;
    s.prop23 = "";
    if (s.prop28) {
        s.eVar28 = s.prop28;
        s.prop28 = "";
    } else {
        s.eVar28 = wminst.Util.getCNNSection(1);
    }
 		if(!s.prop43){ s.prop50 = ""; }
    s.eVar47 = "D=c47";
    s.eVar60 = "D=c60";
    s.prop64 = "";
    s.eVar64 = wminst.Util.getCNNUIEngagement();
    s.eVar21 = wminst.Util.getCNNVideoSequence();
    if (wminst.standardVideoMetadata.isCNNgo) {
        s.prop72 = v.free_preview;
        s.eVar72 = "D=c72";
    } else {
        s.prop72 = s.eVar72 = "";
    }
    s.eVar23 = s.prop23 = "";
    if (!s.prop31) {
        s.eVar31 = "";
    } else {
        s.prop31 = s.prop31.toLowerCase();
    }
    if (s.prop33) {
        s.eVar33 = "D=c33";
    }
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
    vc.set(v.playerid, "vidStarted", true);
    if (wminst.isAMPVideos) {
        s.timestamp = wminst.getAMPVideoTimeStamp();
    }
    if (wminst.isMoneyVideo(v)) {
        wminst.trackMoneyVideoEvent("autostart", v);
    } else {
        wminst.trackLinkVideo(eventType, v);
    }
    s.manageVars("clearVars");
});

/*CNN Video Live*/
PubSub.subscribe("cnnvideo-live", function(data) {
    wminst.isAd = false;
    wminst.isLiveVid = true;
    wminst.is_dvr = false;
    var v = wminst.Util.getVideoMetadata(data);
    s.linkTrackVars = "events,server,channel,prop1,eVar1,prop3,eVar3,prop4,eVar4,prop7,eVar7,prop8,eVar8,prop14,evar14,prop20,eVar20,evar21,eVar22,eVar26,eVar27,eVar28,prop29,eVar29,prop30,eVar30,prop31,eVar31,prop32,eVar32,prop33,eVar33,prop34,eVar34,prop35,eVar35,eVar36,prop37,eVar37,eVar41,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop50,prop52,eVar52,prop54,eVar54,prop56,eVar56,prop57,eVar57,prop59,eVar59,prop64,prop66,eVar66,eVar67,prop68,eVar68,prop70,eVar70,prop71,eVar71,prop72,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    s.linkTrackEvents = "event1,event32";
    s.events = "event1,event32";
    wminst.videoCommonFunction.videoCommonData(v);
    var vc = new wminst.getCNNMediaCollection();
    vc.start(v.playerid, v.title ? v.title : v.headline);
    /*try {
        if (v.category == "live" && v.source.toLowerCase() == "cnn-unauth") {
            s.prop57 = "no mvpd set"; //prop57,eVar57
        }
    } catch (e) {}
    try {
        s.prop59 = v.adobe_hash_id;
    } catch (e) {} */
	if(typeof v.is_autoplay_allowed !== "undefined" &&  typeof v.isAutoStart !== "undefined" && v.is_autoplay_allowed === true && v.isAutoStart === true ){
		wminst.is_autoplay = true;
		if (typeof s.prop70 == "string" && s.prop70 !== ""){
			s.prop70 = s.prop70.replace("noautostart", "autostart");
		}
	}
    if (v.content_type2) { //ngtv player
    } else {
        try {
            if (typeof v.isAutoStart !== 'undefined' && v.isAutoStart == true && ((v.isAutoplayAllowed === true && isAutostartSuccessful !== false) || (v.is_autoplay_allowed === true && v.is_autoplay_successful !== false))) {
                //s.prop70 = "live tv autostart"; //prop70,eVar70
                s.linkTrackEvents = s.linkTrackEvents + ",event34";
                s.events = s.linkTrackEvents;
            }
        } catch (e) {}
    }
    if (!wminst.is_preroll_occur) {
        s.linkTrackEvents = s.linkTrackEvents + ",event90";
        s.events = s.linkTrackEvents;
    }
    s.server = wminst.Util.getADBPURL("domain");  	
    if (!s.prop31) {
        s.eVar31 = "";
    } else {
        s.prop31 = s.prop31.toLowerCase();
    }
    try {
        if (typeof window.CNN.omniture.user_auth_state !== "undefined") {
            var user_auth_state = v.user_auth_state ? v.user_auth_state : window.CNN.omniture.user_auth_state;
            s.prop75 = user_auth_state; //prop75,eVar75
        } else if (typeof v.mvpd !== "undefined" && (v.mvpd == "temppass_cnn10min" || v.mvpd == "TempPass_CNN10min")) {
            s.prop75 = "new_temppass_go"; //prop75,eVar75
        }
    } catch (e) {}
    if (typeof wminst.adVidId !== "undefined" && wminst.adVidId == v.id && wminst.adRange > 0) {
        s.linkTrackEvents = s.linkTrackEvents + ",event3";
        s.events = s.linkTrackEvents;
        wminst.adVidId = "";
    }
    s.eVar23 = s.prop23 = "";
    if (s.pageName) {
        s.eVar26 = s.pageName;
        s.pageName = "";
    } else {
        s.eVar26 = wminst.Util.getCNNPageName();
    }
    if (s.channel) {
        s.eVar27 = s.channel;
        s.channel = "";
    } else {
        s.eVar27 = wminst.Util.getCNNSection(0);
    }
    if (s.prop28) {
        s.eVar28 = s.prop28;
        s.prop28 = "";
    } else {
        s.eVar28 = wminst.Util.getCNNSection(1);
    }
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
 		if(!s.prop43){ s.prop50 = ""; }
    if (typeof v.isLive !== 'undefined' && (v.isLive == true || v.isLive == "true")) { //time limit value set by webdev
        if (wminst.setinterval_id) {
            clearInterval(wminst.setinterval_id);
        } //stop any progress calls
        wminst.setinterval_id = setInterval(function() {
            window.trackVideoProgress(v)
        }, 60000);
    }
    if (wminst.standardVideoMetadata.isCNNGo) {
        s.prop72 = v.free_preview;
    } else {
        s.prop72 = s.eVar72 = "";
    }
    if (wminst.isAMPVideos) {
        s.timestamp = wminst.getAMPVideoTimeStamp();
    }
    wminst.trackLinkVideo("cnnvideo-live", v);
    vc.set(v.playerid, "vidStarted", true);
    s.manageVars("clearVars");
});
