        s.eVar64 = s.prop64;
        s.prop64 = "";
    } else {
        s.eVar64 = wminst.Util.getCNNUIEngagement();
    }
 		if(!s.prop43){ s.prop50 = ""; }
    s.eVar66 = s.prop66;
    s.prop66 = "";
    s.eVar68 = s.prop68;
    s.prop68 = "";
	s.eVar70 = s.prop70;
    s.prop70 = "";
    s.eVar71 = s.prop71;
    s.prop71 = "";
    if (wminst.isLiveVid == true || v.isLive == true || v.isLive == "true") {
        if (v.adType && v.adType.toLowerCase() == "midroll") {
            if (wminst.setinterval_id) {
                clearInterval(wminst.setinterval_id);
            }
        }
    }
    if (wminst.isAMPVideos) {
        s.timestamp = wminst.getAMPVideoTimeStamp();
    }
    if (wminst.isMoneyVideo(v)) {
        wminst.trackMoneyVideoEvent("preroll", v);
    } else {
        s.eVar14 = s.prop14;
        s.prop14 = "";
        wminst.trackLinkVideo("cnnvideo-" + v.adType.toLowerCase(), v);
    }
    try {
        if (v.content_type1 && v.content_type1 == "tve") { //ngtv live, dvr, featured videos
            wminst.setinterval_id = setInterval(function() {
                window.trackVideoProgress(v)
            }, 60000);
        }
    } catch (e) {}
    s.manageVars("clearVars");
});

