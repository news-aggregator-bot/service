			}
		}
        s.eVar1 = s.prop1;
        s.prop1 = "";
        s.eVar3 = s.prop3;
        s.prop3 = "";
        s.eVar4 = s.prop4;
        s.prop4 = "";
        s.eVar7 = s.prop7;
        s.prop7 = "";

        if (s.prop8) {
            s.eVar8 = wminst.Util.getCNNVisitNumber(28);
            s.prop8 = "";
        }
