        ns_.StreamingTag.ContentType = {
            Bumper: "99",
            Live: "13",
            LongFormOnDemand: "12",
            Other: "00",
            ShortFormOnDemand: "11",
            UserGeneratedLive: "23",
            UserGeneratedLongFormOnDemand: "22",
            UserGeneratedShortFormOnDemand: "21"
