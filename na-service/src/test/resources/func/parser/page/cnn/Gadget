         <article class="cd cd--card cd--article cd--idx-6 cd--medium cd--vertical cd--has-siblings cd--has-media cd--media__image" data-vr-contentbox="/2019/09/16/opinions/trump-kavanaugh-justice-department-not-rescue-honig/index.html" data-eq-pts="xsmall: 0, small: 300, medium: 460, large: 780, full16x9: 1100" data-section-name="opinions" data-eq-state="xsmall">
