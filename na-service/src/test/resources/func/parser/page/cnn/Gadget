    wminst.videoCommonFunction.videoCommonData(v);
    s.linkTrackVars = "events,server,channel,eVar26,eVar27,eVar28,prop35,eVar35,list2";
    s.linkTrackEvents = "event48";
    s.events = "event48";
    if (s.pageName) {
        s.eVar26 = s.pageName;
        s.pageName = "";
    } else {
        s.eVar26 = wminst.Util.getCNNPageName();
    }
    if (s.channel) {
        s.eVar27 = s.channel;
        s.channel = "";
    } else {
        s.eVar27 = wminst.Util.getCNNSection(0);
    }
    if (s.prop28) {
        s.eVar28 = s.prop28;
