    var a = data.audio.audio || {};
    var ac = new wminst.getCNNMediaCollection();
    ac.set(a.id, "hasScrubbed", true);
});

/*CNN Audio Progress */
PubSub.subscribe("audio-progress", function(data) {
    var a = data.audio.audio || {};
    var ac = new wminst.getCNNMediaCollection();
    s.linkTrackVars = "events,products,prop1,eVar1,prop2,eVar2,prop7,eVar7,prop29,eVar33,prop35,eVar35,eVar41,eVar42,eVar45,eVar54,eVar67,eVar82";
    s.events = "event98";
    s.linkTrackEvents = s.events;
    wminst.audioCommonData(a);
    var timeSpent = ac.progress(a.id);
 		if (timeSpent > 60)
        wminst.liveAudioInterval = 60;
    timeSpent = wminst.capCNNTimeSpent(timeSpent, a.length, wminst.liveAudioInterval);
    if (timeSpent > 0) {
        s.products = ";;;;" + "event98" + "=" + timeSpent;
    }
    if (timeSpent == 60) {
        wminst.trackLinkAudio("audio-progress", a);
    }
    s.manageVars("clearVars");
});
