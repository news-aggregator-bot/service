/*CNN Video Pause*/
PubSub.subscribe("cnnvideo-pause", function(data) {
    var v = wminst.Util.getVideoMetadata(data);
    var paused = (typeof(v.paused) == "undefined") ? false : v.paused;
    var vc = new wminst.getCNNMediaCollection();
    var prevPaused = vc.get(v.playerid, "isPaused");
    if (paused != prevPaused) {
        vc.pause(v.playerid);
        if (wminst.isLiveVid == true || (v.isLive == true || v.isLive == "true")) { //live video only
            wminst.isLivePaused = paused;
            if (wminst.setinterval_id) {
                clearInterval(wminst.setinterval_id);
            } //stop any progress calls
            if (prevPaused) { //if user resumes, start progress call
                wminst.setinterval_id = setInterval(function() {
                    window.trackVideoProgress(v)
                }, 60000);
            } else { //if user pauses, stop progress call

            }
        }
    }
	wminst.videoCommonFunction.videoCommonData(v);
	s.linkTrackVars ='events,server,channel,list1,prop1,eVar1,prop3,eVar3,prop7,eVar7,prop8,eVar8,prop10,eVar10,prop11,eVar11,prop14,eVar14,prop16,eVar16,eVar26,eVar27,eVar28,prop29,eVar29,prop30,eVar30,prop32,eVar32,prop33,eVar33,prop35,eVar35,eVar36,prop37,eVar37,eVar41,eVar42,prop44,prop46,eVar46,prop47,eVar47,prop52,eVar52,prop54,eVar54,prop56,eVar56,eVar67,prop68,eVar68,prop70,eVar70,eVar196,eVar197'; 
	if (s.prop1){s.eVar1 = s.prop1; s.prop1 = "";}
	if (s.prop3){s.eVar3 = s.prop3; s.prop3 = "";}
	if (s.prop7){s.eVar7 = s.prop7; s.prop7 = "";}
	if (s.prop8){s.eVar8 = s.prop8; s.prop8 = "";} else {s.eVar8 = wminst.Util.getCNNVisitNumber(28);}
	if (s.prop10){s.eVar10 = s.prop10; s.prop10 = "";} else {s.eVar10 = wminst.Util.getCNNDaysSinceLastPublish("a");}
	if (s.prop11){s.eVar11 = s.prop11; s.prop11 = "";} else {s.eVar11 = wminst.Util.getCNNBrandingPartner();}
	if (s.prop14){s.eVar14 = s.prop14; s.prop14 = "";} else {s.eVar14 = wminst.Util.getCNNBrandingSocial();}
	if (s.prop16){s.eVar16 = s.prop16; s.prop16 = "";} else {s.eVar16 = wminst.Util.getCNNPublishDate();}
	if (s.pageName) { s.eVar26 = s.pageName; s.pageName = "";} else {s.eVar26 = wminst.Util.getCNNPageName();}
	if (s.channel) { s.eVar27 = s.channel; s.channel = "";} else {s.eVar27 = wminst.Util.getCNNSection(0);}
	if (s.prop28) { s.eVar28 = s.prop28; s.prop28 = "";} else {s.eVar28 = wminst.Util.getCNNSection(1);}
	if (s.server) {s.eVar29 = s.server; s.server = "";} else {s.eVar29 = wminst.Util.getADBPURL("domain");}
	if (s.prop30) {s.eVar30 = s.prop30; s.prop30 = "";} else {s.eVar30 = wminst.Util.getSiteSpecificSettings(1);}
    if (s.prop32) {s.eVar32 = s.prop32; s.prop32 = "";} else {s.eVar32 = wminst.Util.getCNNTemplateType("long");}
	if (s.prop33) {s.eVar33 = s.prop33; s.prop33 = "";} else {s.eVar33 = wminst.Util.getCNNContentType("adbp:none");}
	if (!s.prop35) {s.prop35 = wminst.Util.getCNNCodeVersion();}
	if (s.prop37) {s.eVar37 = s.prop37; s.prop37 = "";} else {s.eVar37 = wminst.Util.getCNNPlatform();}
	if (s.prop29) {s.eVar41 = s.prop29; s.prop29 = "";}
	if (s.prop52) {s.eVar52 = s.prop52; s.prop52 = "";}
	if (s.prop54) {s.eVar54 = s.prop54; s.prop54 = "";}
    if(!s.list1) {s.list1 = wminst.Util.getCEPTopisForVideo(v) || wminst.Util.getCNNTopic();}
  try {
		s.eVar22 = s.prop23 = s.eVar23 = s.prop31 = s.eVar31 = s.prop34 = s.eVar34 = s.prop59 = s.eVar59 = s.prop64 = s.list2 = s.prop66 = s.eVar66 = s.prop71 = s.eVar71 = s.prop72 = s.eVar72 = s.prop73 = s.eVar73 = s.prop75 = s.eVar75 = "";
	} catch(e){}
  if (s.prop68) {s.eVar68 = s.prop68; s.prop68 = "";}
  if (wminst.is_autoplay === true){
		if (typeof s.prop70 == "string" && s.prop70 !== ""){
			s.prop70 = s.prop70.replace("noautostart", "autostart");
		}
	}
	if (s.prop70) {
		s.eVar70 = s.prop70; s.prop70 = "";
	}
	var pauseStatus = '';
	if (paused == 'true' || paused == true) {
		s.linkTrackEvents = 'event55';
		pauseStatus = 'pause'; 
	} else if (paused == 'false' || paused == false) {
		s.linkTrackEvents = 'event56';
		pauseStatus = 'resume';
	}
	s.events = s.linkTrackEvents;
    if (wminst.isMoneyVideo(v)) {
      /* Do Nothing*/
    } else {
        wminst.trackLinkVideo('cnnvideo-' + pauseStatus, v);
    }
	s.manageVars('clearVars');
});

/*CNN Video Buffer*/
PubSub.subscribe("cnnvideo-buffer", function(data) {
    var v = wminst.Util.getVideoMetadata(data);
    var buffering = (typeof(v.buffering) == "undefined") ? false : v.buffering;
    var vc = new wminst.getCNNMediaCollection();
    var prevBuffering = vc.get(v.playerid, "isBuffering");
    if (buffering != prevBuffering) {
        vc.buffer(v.playerid);
        if (wminst.isLiveVid == true || (v.isLive == true || v.isLive == "true")) { //live video only
            if (wminst.setinterval_id) {
                clearInterval(wminst.setinterval_id);
            } //stop any progress calls
            if (prevBuffering) { //stop buffering, start progress call
                wminst.setinterval_id = setInterval(function() {
                    window.trackVideoProgress(v)
                }, 60000);
            } else { //buffering, stop progress call

            }
        }
    }
});
/*CNN Video Scrub */
PubSub.subscribe("cnnvideo-scrub", function(data) {
    var v = wminst.Util.getVideoMetadata(data);
    var vc = new wminst.getCNNMediaCollection();
    vc.set(v.playerid, "hasScrubbed", true);
    wminst.is_dvr = true;
});

/*CNN Video Progress */
PubSub.subscribe("cnnvideo-progress", function(data) {
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    var vc = new wminst.getCNNMediaCollection();
    var timeSpent = vc.progress(v.playerid);
    wminst.live_interval = 60;
    timeSpent = wminst.capCNNTimeSpent(timeSpent, v.trt, wminst.live_interval);
    s.linkTrackVars = "events,products,eVar1,eVar3,eVar4,eVar7,eVar8,eVar14,eVar20,eVar21,eVar26,eVar27,eVar28,eVar29,eVar30,prop31,eVar31,eVar32,eVar33,eVar34,prop35,eVar35,eVar36,eVar37,eVar41,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop50,eVar52,eVar53,eVar54,prop56,eVar56,eVar57,prop59,eVar59,prop64,eVar64,eVar66,eVar67,eVar68,eVar70,eVar71,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    s.linkTrackEvents = "event36";
    s.events = "event36";
    s.eVar1 = s.prop1;
    s.prop1 = "";
    s.eVar3 = s.prop3;
    s.prop3 = "";
    s.eVar4 = s.prop4;
    s.prop4 = "";
    s.eVar7 = s.prop7;
    s.prop7 = "";
    if (s.prop8) {
        s.eVar8 = s.prop8;
        s.prop8 = "";
    } else {
        s.eVar8 = wminst.Util.getCNNVisitNumber(28);
    }
	if (wminst.is_autoplay === true){
		if (typeof s.prop70 == "string" && s.prop70 !== ""){
			s.prop70 = s.prop70.replace("noautostart", "autostart")
		}
	}
    s.eVar10 = s.prop10;
    s.prop10 = "";
    s.eVar11 = s.prop11;
    s.prop11 = "";
    s.eVar14 = s.prop14;
    s.prop14 = "";
    s.eVar16 = s.prop16;
    s.prop16 = "";
    s.eVar20 = s.prop20;
    s.prop20 = "";
    s.eVar23 = s.prop23 = "";
    if (s.channel) {
        s.eVar27 = s.channel;
        s.channel = "";
    } else {
        s.eVar27 = wminst.Util.getCNNSection(0);
    }
    s.eVar28 = wminst.Util.getCNNSection(1);
    if (s.pageName) {
        s.eVar26 = s.pageName;
        s.pageName = "";
    } else {
        s.eVar26 = wminst.Util.getCNNPageName();
    }
    if (s.server) {
        s.eVar29 = s.server;
        s.server = "";
    } else {
        s.eVar29 = wminst.Util.getADBPURL("domain");
    }
    s.eVar30 = s.prop30;
    s.prop30 = "";
    s.eVar41 = s.prop29;
    if (s.prop31) {
        s.eVar31 = s.prop31.toLowerCase();
        s.prop31 = "";
    } else {
        s.eVar31 = "";
    }
    if (s.prop32) {
        s.eVar32 = s.prop32;
    } else {
        s.eVar32 = wminst.Util.getCNNTemplateType();
    }
    s.prop32 = "";
    if (s.prop33) {
        s.eVar33 = s.prop33;
        s.prop33 = "";
    }
    s.eVar34 = s.prop34;
    s.prop34 = "";
    if (!s.prop35) {
        s.prop35 = wminst.Util.getCNNCodeVersion();
    }
    s.eVar37 = s.prop37;
    s.prop37 = "";
 		if(!s.prop43){ s.prop50 = ""; }
    s.eVar52 = s.prop52;
    s.prop52 = "";
    s.eVar53 = s.prop53;
    s.prop53 = "";
    s.eVar54 = s.prop54;
    s.prop54 = "";
	s.eVar56 = s.prop56;
    s.prop56 = "";
    s.eVar57 = s.prop57;
    s.prop57 = "";
    s.eVar64 = s.prop64;
    s.prop64 = "";
    s.eVar66 = s.prop66;
    s.prop66 = "";
    s.eVar68 = s.prop68;
    s.prop68 = "";
	s.eVar70 = s.prop70;
    s.prop70 = "";
    s.eVar71 = s.prop71;
    s.prop71 = "";
    s.eVar72 = s.prop72;
    s.prop72 = "";
    s.products = ";;;;" + s.events + "=" + timeSpent;
    if (wminst.isAMPVideos) {
        s.timestamp = wminst.getAMPVideoTimeStamp();
    }
    if (timeSpent == 60 && !wminst.isLivePaused) {
        wminst.trackLinkVideo("cnnvideo-progress", v);
    }
    s.manageVars("clearVars");
});


/*CNN Video Ten */
PubSub.subscribe("cnnvideo-ten", function(data) {
    wminst.isAd = false;
    var v = wminst.Util.getVideoMetadata(data);
    if (wminst.isMoneyVideo(v))
        return;
    var vc = new wminst.getCNNMediaCollection();
    if (!vc.get(v.playerid, "isTen") && !vc.get(v.playerid, "hasScrubbed")) {
        vc.set(v.playerid, "isTen", true);
        var timeSpent = vc.progress(v.playerid);
        wminst.videoCommonFunction.videoCommonData(v);
        s.linkTrackVars = "events,server,channel,products,eVar1,eVar3,eVar4,eVar7,eVar8,eVar10,eVar11,eVar14,eVar16,eVar20,eVar22,prop23,eVar23,eVar26,eVar27,eVar28,eVar41,eVar29,eVar30,prop31,eVar31,eVar32,eVar33,eVar34,prop35,eVar35,eVar36,eVar37,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop50,eVar52,eVar53,eVar54,prop56,eVar56,eVar57,prop59,eVar59,eVar60,eVar64,eVar66,eVar67,eVar68,eVar70,eVar71,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
        s.linkTrackEvents = "event40";
        s.events = "event40";
        s.products = "";
        timeSpent = wminst.capCNNTimeSpent(timeSpent, v.trt, wminst.live_interval);
        if (timeSpent > 0) {
            s.linkTrackEvents = "event40,event36";
            s.events = "event40,event36";
            s.products = ";;;;" + "event36" + "\x3d" + timeSpent;
        }
		if(wminst.is_autoplay === true){
			if (typeof s.prop70 == "string" && s.prop70 !== ""){
				s.prop70 = s.prop70.replace("noautostart", "autostart")
			}
		}
        s.eVar1 = s.prop1;
        s.prop1 = "";
        s.eVar3 = s.prop3;
        s.prop3 = "";
        s.eVar4 = s.prop4;
        s.prop4 = "";
        s.eVar7 = s.prop7;
        s.prop7 = "";

        if (s.prop8) {
            s.eVar8 = wminst.Util.getCNNVisitNumber(28);
            s.prop8 = "";
        }
        s.eVar10 = s.prop10;
        s.prop10 = "";
        s.eVar11 = s.prop11;
        s.prop11 = "";
        s.eVar14 = s.prop14;
        s.prop14 = "";
        s.eVar16 = s.prop16;
        s.prop16 = "";
        s.eVar20 = s.prop20;
        s.prop20 = "";
        s.eVar23 = s.prop23;
        s.prop23 = "";
        if (s.pageName) {
            s.eVar26 = s.pageName;
            s.pageName = "";
        } else {
            s.eVar26 = wminst.Util.getCNNPageName();
        }
        s.eVar27 = s.channel;
        s.channel = "";
        if (s.prop28) {
            s.eVar28 = wminst.Util.getCNNSection(1);
            s.prop28 = "";
        } else {
            s.eVar28 = wminst.Util.getCNNSection(1);
        }
        if (s.server) {
            s.eVar29 = s.server;
            s.server = "";
        } else {
            s.eVar29 = wminst.Util.getADBPURL("domain");
        }
        s.eVar30 = s.prop30;
        s.prop30 = "";
        s.eVar41 = s.prop29;
        if (s.prop31) {
            s.eVar31 = s.prop31.toLowerCase();
            s.prop31 = "";
        } else {
            s.eVar31 = "";
        }
        s.eVar32 = s.prop32;
        s.prop32 = "";
        if (s.prop33) {
            s.eVar33 = s.prop33;
            s.prop33 = "";
        }
        s.eVar34 = s.prop34;
        s.prop34 = "";
        if (!s.prop35) {
            s.prop35 = wminst.Util.getCNNCodeVersion();
        }
        s.eVar37 = s.prop37;
        s.prop37 = "";
     		if(!s.prop43){ s.prop50 = ""; }
        s.eVar47 = "D=c47";
        s.eVar52 = s.prop52;
        s.prop52 = "";
        s.eVar53 = s.prop53;
        s.prop53 = "";
		s.eVar56 = s.prop56;
        s.prop56 = "";
        s.eVar54 = s.prop54;
        s.prop54 = "";
        if (s.prop57) {
            s.eVar57 = s.prop57;
            s.prop57 = "";
        } 
        s.eVar60 = s.prop60;
        s.prop60 = "";
        s.eVar64 = wminst.Util.getCNNUIEngagement();
        s.prop64 = "";
        s.eVar66 = s.prop66;
        s.prop66 = "";
        s.eVar68 = s.prop68;
        s.prop68 = "";
		s.eVar70 = s.prop70;
        s.prop70 = "";
        s.eVar71 = s.prop71;
        s.prop71 = "";
        s.eVar72 = s.prop72;
        s.prop72 = "";
        if (wminst.isAMPVideos) {
            s.timestamp = wminst.getAMPVideoTimeStamp();
        }
        wminst.trackLinkVideo("cnnvideo-ten", v);
        s.manageVars("clearVars");
    }
});

