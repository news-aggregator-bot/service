/*CNN Audio Pause-Resume */
PubSub.subscribe("audio-pause", function(data) {
    var a = data.audio.audio || {};
    var paused = (typeof(a.pause) == "undefined") ? false : a.pause;
    var ac = new wminst.getCNNMediaCollection();
		if (ac.get(a.id, 'audStarted')) {
			s.manageVars('clearVars');
			s.linkTrackVars ='events,prop1,eVar1,prop2,eVar2,prop7,eVar7,prop29,eVar33,prop35,eVar35,eVar41,eVar42,eVar45,eVar54,eVar67,eVar82';
			s.events = 'event100';
			if (paused) {
				s.events = 'event99';
			}
			s.linkTrackEvents = s.events;
			wminst.audioCommonData(a);
			wminst.trackLinkAudio(paused ? 'audio-pause' : 'audio-resume', a);
		}
    var prevPaused = ac.get(a.id, "isPaused");
    if (paused != prevPaused) {
        ac.pause(a.id);
        if (wminst.setAudioInterval_id) {
            clearInterval(wminst.setAudioInterval_id);
        } //stop any progress calls
        if (prevPaused) { //if user resumes, start progress call
            wminst.setAudioInterval_id = setInterval(function() {
                window.trackAudioProgress(data)
            }, 60000);
        } else { //if user pauses, stop progress call

        }
    }
	s.manageVars('clearVars');
});

/*CNN Audio Buffer*/
PubSub.subscribe("audio-buffer", function(data) {
    var a = data.audio.audio || {};
    var buffering = (typeof(a.buffering) == "undefined") ? false : a.buffering;
    var ac = new wminst.getCNNMediaCollection();
    var prevBuffering = ac.get(a.id, "isBuffering");
    if (buffering != prevBuffering) {
        ac.buffer(a.id);
        if (wminst.setAudioInterval_id) {
            clearInterval(wminst.setAudioInterval_id);
        } //stop any progress calls
        if (prevBuffering) { //stop buffering, start progress call
            wminst.setAudioInterval_id = setInterval(function() {
                window.trackAudioProgress(data)
            }, 60000);
        } else { //buffering, stop progress call

        }
    }
});

