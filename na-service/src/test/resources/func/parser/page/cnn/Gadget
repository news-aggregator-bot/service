     <div class="zn-header">
      <h2 class="zn-header__text">Voices<span class="zn-header__stripes">&nbsp;</span></h2>
     </div>
     <div class="zn__containers">
      <div class="column zn__column--idx-0">
       <div id="cn-container_172BDE1D-5028-12C5-3938-9E7CEF933727" class="cn cn-carousel-medium-strip cn--idx-0 cn-container_172BDE1D-5028-12C5-3938-9E7CEF933727" data-layout="carousel-medium-strip">
        <div class="loading-video-carousel"></div>
        <div class="js-owl-carousel owl-carousel carousel--fixed carousel-medium-strip owl-loaded owl-drag" data-galleryname="" data-cut-format="" data-is-gallery="" data-slide-count="37">
         <div class="owl-stage-outer">
          <div class="owl-stage" style="transform: translate3d(-3040px, 0px, 0px); transition: all 0s ease 0s; width: 12000px;">
           <div class="owl-item cloned" style="width: auto;">
            <div class="cn__column carousel__content__item">
             <article class="cd cd--card cd--profile cd--idx-18 cd--medium cd--vertical cd--has-siblings cd--has-media cd--media__image" data-vr-contentbox="/profiles/robert-klitzman" data-eq-pts="xsmall: 0, small: 300, medium: 460, large: 780, full16x9: 1100" data-eq-state="xsmall">
