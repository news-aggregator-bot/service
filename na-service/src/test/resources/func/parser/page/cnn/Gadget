    s.prop71 = "";
    if (wminst.isLiveVid == true || v.isLive == true || v.isLive == "true") {
        if (v.adType && v.adType.toLowerCase() == "midroll") {
            if (wminst.setinterval_id) {
                clearInterval(wminst.setinterval_id);
            }
        }
