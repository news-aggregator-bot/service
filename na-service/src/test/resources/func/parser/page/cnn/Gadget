PubSub.subscribe("cnnvideo-preroll", function(data) {
    wminst.isAd = true;
    var v = wminst.Util.getVideoMetadata(data);
    wminst.videoCommonFunction.videoCommonData(v);
    wminst.adVidId = v.id;
    s.linkTrackVars = "events,server,channel,eVar1,eVar3,eVar4,eVar7,eVar8,eVar10,eVar11,eVar14,eVar16,eVar20,eVar22,eVar26,eVar27,eVar28,eVar29,eVar30,prop31,eVar31,eVar32,eVar33,eVar34,prop35,eVar35,eVar36,eVar37,eVar41,eVar42,prop43,prop44,prop46,eVar46,prop47,eVar47,prop49,eVar49,prop50,eVar52,eVar53,eVar54,eVar56,eVar57,prop59,eVar59,eVar60,eVar64,eVar66,eVar67,eVar68,eVar70,eVar72,prop73,eVar73,prop75,eVar75,list1,list2";
    s.linkTrackEvents = "event35";
    s.events = "event35";
    if (typeof v.adType == "string" && v.adType.toLowerCase() == "midroll") {
		s.linkTrackEvents = "event25";
		s.events = "event25";
    }
    if (typeof v.adType == "string" && v.adType.toLowerCase() == "preroll") {
        s.linkTrackEvents = s.linkTrackEvents + ",event90";
        s.events = s.linkTrackEvents;
        wminst.is_preroll_occur = true;
    }
    if (v.category && v.category !== "live" || v.isLive !== "true" || v.isLive !== true) {
        wminst.isLiveVid = false;
    }
    if (wminst.standardVideoMetadata.isCNNGo) {
        s.eVar72 = v.free_preview;
