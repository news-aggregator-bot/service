         <article class="cd cd--card cd--article cd--idx-35 cd--small cd--vertical cd--has-siblings cd--has-media cd--media__image" data-vr-contentbox="/2016/10/18/opinions/cartooning-through-the-presidential-campaign-thomas-mcclure/index.html" data-eq-pts="xsmall: 0, small: 300, medium: 460, large: 780, full16x9: 1100" data-section-name="arts" data-eq-state="xsmall">
