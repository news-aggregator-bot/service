  <script>_satellite["__runScript5"](function(event, target) {
wminst.nielsenPageBeacon = function() {
    var ci, si, rp, random;
    ci = "us-204044h";
    si = escape(window.location.href);
    rp = escape(document.referrer);
    random = +(new Date());
