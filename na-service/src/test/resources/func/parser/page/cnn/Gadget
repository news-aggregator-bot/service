         <article class="cd cd--card cd--article cd--idx-13 cd--small cd--vertical cd--has-siblings cd--has-media cd--media__image" data-vr-contentbox="/2018/09/22/opinions/anthony-bourdain-parts-unknown-censor-spicer-joslyn/index.html" data-eq-pts="xsmall: 0, small: 300, medium: 460, large: 780, full16x9: 1100" data-section-name="opinions" data-eq-state="xsmall">
