       <script>

    var adBlocked = '1'; 
    if (typeof abp !== 'undefined') {
        if (abp == false || adp == '0') {  
            var adBlocked = '0'; 
        }
    }
    var topic_channel_tag = document.querySelector('meta[name="analyticsAttributes.topicChannel"]');
    var topic_sub_channel_tag = document.querySelector('meta[name="analyticsAttributes.topicSubChannel"]');
    var content_type_tag = document.querySelector('meta[name="analyticsAttributes.contentType"]');
    var content_channel_tag = document.querySelector('meta[name="analyticsAttributes.contentChannel"]');
    var edition_tag = document.querySelector('meta[name="analyticsAttributes.edition"]');
    var keywords_tag = document.querySelector('meta[name="keywords"]');
    var canonical_link = document.querySelector('link[rel="canonical"]');

    var topic_channel = topic_channel_tag && topic_channel_tag.content;
    var topic_sub_channel = topic_sub_channel_tag && topic_sub_channel_tag.content;
    var content_type = content_type_tag && content_type_tag.content;
    var content_channel = content_channel_tag && content_channel_tag.content;
    var edition = edition_tag && edition_tag.content;
    var keywords = keywords_tag && keywords_tag.content;
    var canonical = canonical_link && canonical_link.href;

    var category = 'Landing Page';
    if (content_type === 'Homepage') {
        var category = 'Homepage';
    }
    if (content_type === 'Slideshow') {
        var category = 'Slideshow';
    }

    var master_list = {
        canonical_url: canonical,
        content_channel: content_channel,
        content_type: content_type,
        content_type_channel: content_channel,
        edition: edition,
        keywords: keywords,
        topic_channel: topic_channel,
        topic_sub_channel: topic_sub_channel,
        ad_blocked: adBlocked,
        platform: 'Desktop',
        category: category
    };

    // Wait for elements to load because this
    // can't be inserted below the elements
    // globally in the CMS
    setTimeout(function() {
        //
        // Active Time Spent Tracking
        //
        if (window.analytics) {
            analytics.track('active.time.spent.first_interaction', {
                ...master_list,
                label: '0',
                nonInteraction: '1',
                activeTimeSpentFirstInteraction: '1'
            });
        }

        //
        // Click Tracking
        //
        var hero_headline = document.querySelector('.story.featured-article');
        var top_story = document.querySelector('.topStory');
        var feed_headlines = document.querySelectorAll('.news-headline-list .story');
        var more_headlines = document.querySelectorAll('.more-headlines a');
        var view_more = document.querySelector('.control-nav-next');
        var right_rail_links = document.querySelectorAll('.column2 .mod-video a');
        var video_module_links = document.querySelectorAll('.video-module .video');

        if (window.analytics) {
            if (hero_headline) {
                var headline = hero_headline.querySelector('.story-title');
                var label = headline && headline.innerText;
                hero_headline.addEventListener('click', function(e) {
                    analytics.track('ui_interaction', {
                        ...master_list,
                        ui_element: 'Hero Content',
                        label: label,
                        nonInteraction: '0',
                        uiInteraction: '1',
                    });
                })
            }
            if (top_story) {
                var headline = top_story.querySelector('h2');
                var label = headline && headline.innerText;
                top_story.addEventListener('click', function(e) {
                    analytics.track('ui_interaction', {
                        ...master_list,
                        ui_element: 'Hero Content',
                        label: label,
                        nonInteraction: '0',
                        uiInteraction: '1',
                    });
                })
            }
            if (feed_headlines) {
                for (i=0; i < feed_headlines.length; i++) {
                    if (feed_headlines[i].classList.value.indexOf('featured-article') === -1) {
                        feed_headlines[i].addEventListener('click', function() {
                            var headline = this.querySelector('.story-title');
                            var label = headline && headline.innerText;
                            analytics.track('ui_interaction', {
                                ...master_list,
                                ui_element: 'Feed Content',
                                label: label,
                                nonInteraction: '0',
                                uiInteraction: '1',
                            });
                        });
                    }
                }
            }
            if (more_headlines) {
                for (i=0; i < more_headlines.length; i++) {
                    more_headlines[i].addEventListener('click', function() {
                        var label = more_headlines[i].innerText;
                        analytics.track('ui_interaction', {
                            ...master_list,
                            ui_element: 'Feed Content',
                            label: label,
                            nonInteraction: '0',
                            uiInteraction: '1',
                        });
                    });
                }
            }
            if (view_more) {
                view_more.addEventListener('click', function(e) {
                    analytics.track('ui_interaction', {
                        ...master_list,
                        ui_element: 'Next Page',
                        label: 'View More',
                        nonInteraction: '0',
                        uiInteraction: '1',
                    });
                })
            }
            if (right_rail_links) {
                for (i=0; i < right_rail_links.length; i++) {
                    right_rail_links[i].addEventListener('click', function() {
                        var label = this.innerText;
                        analytics.track('ui_interaction', {
                            ...master_list,
                            ui_element: 'Right Rail',
                            label: label,
                            nonInteraction: '0',
                            uiInteraction: '1',
                        });
                    });
                }
            }
            if (video_module_links) {
                for (i=0; i < video_module_links.length; i++) {
                    video_module_links[i].addEventListener('click', function(e) {
                        var label = e.target.innerText || e.target.alt;
                        analytics.track('ui_interaction', {
                            ...master_list,
                            ui_element: 'Video Module',
                            label: label,
                            nonInteraction: '0',
                            uiInteraction: '1',
                        });
                    });
                }
            }
        }
    }, 2000);

