                function loadWeather() {
                    $.ajax({
                        type: 'GET',
                        url: 'widget/weather.hnd',
                        success: function (data) {
                            $('#weather-widget').html(data);
                            $(".dropdown-menu li>ul>li").click(function () {
                                $.cookie("city", $(this).attr('id'), { expires: 365, domain: Domain.Cookie, path: '/' });
                                loadWeather();
                            });
                        }
                    });
                }                                             
            </script> 
