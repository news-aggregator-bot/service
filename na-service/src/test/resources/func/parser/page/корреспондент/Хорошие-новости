    <script type="text/javascript">
    (function() {
        var domain = window.location.hostname,
            partition = window.location.pathname.split('/')[1],
            subPartition = window.location.pathname.split('/')[2],
            container = document.getElementById('for_ph_rm'),
            ph = document.createElement('DIV'),
            pid = 'admixer_rm_inner', // other pages
            dataZone = "0c05bfe1-54d5-4247-b91a-f9da0c4807fe"; // other pages
        container.appendChild(ph);
        if ((domain=='korrespondent.net')||(domain=='ua.korrespondent.net')) {
            // main
            if (partition == '') {
                pid = 'admixer_rm_main';
                dataZone = "5367f7aa-e4a4-4553-8f35-ab84c3212209";
                // sport
            } else if (partition == 'sport') {
                pid = 'admixer_rm_sport';
                dataZone = "d9610585-aad9-4f2c-a07d-b65ced65f753";
                // lifestyle
            } else if (partition == 'lifestyle') {
                // lifestyle/motors
                if (subPartition == 'motors') {
                    pid = 'admixer_rm_lifestyle_motors';
                    dataZone = "79ed49b5-f5f7-4842-bf5e-107afadc4d71";
                    // lifestyle/interiors
                } else if (subPartition == 'interiors') {
                    pid = 'admixer_rm_lifestyle_interiors';
                    dataZone = "67b0e1d1-bd84-4819-afe7-62015b4edbe6";
                    // lifestyle/other
                } else {
                    pid = 'admixer_rm_lifestyle';
                    dataZone = "284bffd9-df61-44aa-8c93-274bf36127a8";
                }
                // business
            } else if (partition == 'business') {
                pid = 'admixer_rm_business';
                dataZone = "ae4a30c7-5a10-442d-a4ca-e8b838481a80";
            }
        }
        ph.id = pid;
        ph.setAttribute('data-zone', dataZone);
        (window.admixerAds = window.admixerAds || []).push(ph.id);
    })();
</script> 
