var neededHeight_3_16_NewsPartList;var currentHeight_3_16_NewsPartList;function align_3_16_NewsPartList(){if(neededHeight_3_16_NewsPartList == $('.col__sidebar').outerHeight() && currentHeight_3_16_NewsPartList == $('.col__main').outerHeight()){return;}
neededHeight_3_16_NewsPartList = $('.col__sidebar').outerHeight();
currentHeight_3_16_NewsPartList = $('.col__main').outerHeight();
var difference = currentHeight_3_16_NewsPartList - neededHeight_3_16_NewsPartList;
while(difference > 0 && difference > $('.article.article_rubric_top:visible').last().outerHeight() *0.9 || difference<0 && (-difference)>$('.article.article_rubric_top:visible').last().outerHeight()*0.3){
if(difference > 0 && $('.article.article_rubric_top:visible').length > 0){
if((currentHeight_3_16_NewsPartList - $('.article.article_rubric_top:visible').last().outerHeight())>1100){$('.article.article_rubric_top:visible').last().hide();
}else{break;}
}else if ($('.article.article_rubric_top:hidden').length > 0 && difference < 0){$('.article.article_rubric_top:hidden').first().show();}else{break;}
currentHeight_3_16_NewsPartList = $('.col__main').outerHeight();difference = currentHeight_3_16_NewsPartList - neededHeight_3_16_NewsPartList;}
}
$(document).ready(function () {

$(window).scroll(align_3_16_NewsPartList);});
</script> 
