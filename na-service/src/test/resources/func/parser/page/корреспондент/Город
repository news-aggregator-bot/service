  <script type="text/javascript">
	(function() {
		var domain = window.location.hostname,
			partition = window.location.pathname.split('/')[1],
			subPartition = window.location.pathname.split('/')[2],
			container = document.getElementById('for_ph_mobile'),
			ph = document.createElement('DIV'),
			pid = 'admixer_mobile_inner', // other pages
			dataZone = "e5fd91e8-bd2f-4dfd-8828-56f30e9914b6"; // other pages
		container.appendChild(ph);
		if ((domain=='korrespondent.net')||(domain=='ua.korrespondent.net')) {
			// main
			if (partition == '') {
				pid = 'admixer_mobile_main';
				dataZone = "bc30f3a2-841c-4e0a-98e4-9885350a4012";
			// sport
			} else if (partition == 'sport') {
				pid = 'admixer_mobile_sport';
				dataZone = "53b4a9af-9b8f-4fc7-8d14-43ff64cac1ca";
			// lifestyle
			} else if (partition == 'lifestyle') {
				// lifestyle/motors
				if (subPartition == 'motors') {
					pid = 'admixer_mobile_lifestyle_motors';
					dataZone = "686aa21f-6a70-4a52-a4d3-1267c153d06a";
				// lifestyle/interiors
				} else if (subPartition == 'interiors') {
					pid = 'admixer_mobile_lifestyle_interiors';
					dataZone = "84ebcba7-4bff-41be-b9d7-60ebb44faa68";
				// lifestyle/other
				} else {
					pid = 'admixer_mobile_lifestyle';
					dataZone = "3b116799-ef27-4c28-9ac6-4fe16e56afc4";
				}
			// business
			} else if (partition == 'business') {
				pid = 'admixer_mobile_business';
				dataZone = "d8b047b5-0615-48b0-adef-c9b63bf52464";
			}
		}
		ph.id = pid;
		ph.setAttribute('data-zone', dataZone);
		(window.admixerAds = window.admixerAds || []).push(ph.id);
	})();
