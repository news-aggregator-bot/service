    <script type="text/javascript">
                $(function () {
                    $('.selectpicker').selectpicker();
                });

                function converter() {
                        var sum = $("#fromValue").val();
                        var course = $("#_8_17_ddlCurrencyFrom").val();
                        var to = $("#_8_17_ddlCurrencyTo").val();
                        $("#toValue").val(calc(sum, course, to));

                        function calc(sum, course, to) {
                            if (sum != '') {
                                return (Number(sum.replace(",", ".")) * Number(course.replace(",", ".")) / Number(to.replace(",", "."))).toFixed(2);
                            }
                            return '';
                        }
                    }                                
            </script> 
