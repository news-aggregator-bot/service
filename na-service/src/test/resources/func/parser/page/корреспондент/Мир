					dataZone = "686aa21f-6a70-4a52-a4d3-1267c153d06a";
				// lifestyle/interiors
				} else if (subPartition == 'interiors') {
					pid = 'admixer_mobile_lifestyle_interiors';
					dataZone = "84ebcba7-4bff-41be-b9d7-60ebb44faa68";
				// lifestyle/other
				} else {
					pid = 'admixer_mobile_lifestyle';
					dataZone = "3b116799-ef27-4c28-9ac6-4fe16e56afc4";
				}
			// business
			} else if (partition == 'business') {
				pid = 'admixer_mobile_business';
				dataZone = "d8b047b5-0615-48b0-adef-c9b63bf52464";
			}
		}
		ph.id = pid;
		ph.setAttribute('data-zone', dataZone);
		(window.admixerAds = window.admixerAds || []).push(ph.id);
	})();
</script><!-- (C)2000-2014 Gemius SA - gemiusAudience / korrespondent.net / World --> 
