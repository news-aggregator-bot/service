l,&quot;freeField2&quot;:&quot;&quot;,&quot;type&quot;:&quot;&quot;,&quot;program&quot;:{&quot;id&quot;:&quot;world&quot;,&quot;urlSafeValue&quot;:&quot;world&quot;,&quot;title&quot;:&quot;world news&quot;,&quot;online&quot;:&quot;1&quot;,&quot;url&quot;:&quot;\/\/www.euronews.com\/programs\/world&quot;},&quot;vertical&quot;:&quot;news&quot;,&quot;verticals&quot;:[{&quot;id&quot;:&quot;1&quot;,&quot;slug&quot;:&quot;news&quot;,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;}],&quot;primaryVertical&quot;:{&quot;id&quot;:&quot;1&quot;,&quot;slug&quot;:&quot;news&quot;,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;},&quot;themes&quot;:[{&quot;id&quot;:&quot;1&quot;,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;,&quot;translation&quot;:&quot;World&quot;,&quot;url&quot;:&quot;\/\/www.euronews.com\/news\/international&quot;}],&quot;primaryTheme&quot;:{&quot;id&quot;:&quot;1&quot;,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;},&quot;advertising&quot;:0,&quot;advertisingData&quot;:{&quot;startDate&quot;:0,&quot;endDate&quot;:0,&quot;type&quot;:null,&quot;isDfp&quot;:null,&quot;slug&quot;:null,&quot;title&quot;:null,&quot;disclaimerLabelKey&quot;:null,&quot;sponsor&quot;:null,&quot;sponsorName&quot;:null,&quot;sponsorUrl&quot;:null,&quot;sponsorLogo&quot;:&quot;&quot;,&quot;sponsorLogoReverse&quot;:&quot;&quot;},&quot;geoLocation&quot;:{&quot;lat&quot;:0,&quot;lon&quot;:0},&quot;location&quot;:1,&quot;continent&quot;:{&quot;id&quot;:&quot;104&quot;,&quot;urlSafeValue&quot;:&quot;europe&quot;,&quot;title&quot;:&quot;Europe&quot;},&quot;country&quot;:{&quot;id&quot;:&quot;145&quot;,&quot;urlSafeValue&quot;:&quot;iceland&quot;,&quot;title&quot;:&quot;Iceland&quot;,&quot;url&quot;:&quot;\/news\/europe\/iceland&quot;},&quot;town&quot;:[],&quot;versions&quot;:[],&quot;fullUrl&quot;:&quot;\/2019\/08\/12\/iceland-s-first-glacier-to-disappear-due-to-climate-change-will-be-remembered-with-monumen&quot;,&quot;lastModified&quot;:1565704483},{&quot;id&quot;:&quot;835422&quot;,&quot;cid&quot;:&quot;4080934&quot;,&quot;versionId&quot;:&quot;1&quot;,&quot;archive&quot;:0,&quot;housenumber&quot;:&quot;190809_NCSU_8769777&quot;,&quot;owner&quot;:&quot;euronews&quot;,&quot;isMagazine&quot;:&quot;0&quot;,&quot;channels&quot;:[{&quot;id&quot;:&quot;1&quot;},{&quot;id&quot;:&quot;2&quot;},{&quot;id&quot;:&quot;4&quot;},{&quot;id&quot;:&quot;5&quot;},{&quot;id&quot;:&quot;12&quot;}],&quot;status&quot;:2,&quot;title&quot;:&quot;Residents paint street in colours of rainbow flag to celebrate Reykjavik Pride&quot;,&quot;titleSeo&quot;:null,&quot;titleListing1&quot;:&quot;Residents paint street in colours of rainbow flag for Reykjavik Pride&quot;,&quot;titleListing2&quot;:&quot;Residents paint street in colours of rainbow flag to celebrate Reykjavik Pride&quot;,&quot;leadin&quot;:&quot;Reykjavik Pride has kicked off in Iceland, with events set to take place over the next ten days.&quot;,&quot;summary&quot;:&quot;Reykjavik Pride has kicked off in Iceland, with events set to take place over the next ten days.&quot;,&quot;keySentence&quot;:null,&quot;url&quot;:&quot;residents-paint-street-in-colours-of-rainbow-flag-to-celebrate-reykjavik-pride&quot;,&quot;masterCms&quot;:&quot;v2&quot;,&quot;plainText&quot;:&quot;Reykjavik Pride has kicked off in Iceland, with events set to take place over the next ten days. \n\nThis is the 20th edition of the festival in the country.\u00a0 \n\n\nThe pre-opening ceremony took place in an unusual location\u00a0\u2014 the Icelandic Stock Exchange. \n\nReykjavik Pride\u00b4's president, Gunnlaugur Bragi Bj\u00f6rnsson, rang the exchange's bell along with Eva Maria Lange, of the organisation Pink Iceland and Pall Hardarson, president of Nasdaq Iceland, on Thursday morning.\u00a0 \n\nThe move was intended to bring into focus the status of LGBTQIA people in the business world. \n\nFor the fifth year in a row, a street in Reykjavik was chosen to be painted in the colours of the rainbow flag. \n\nThis time, it was Klapparstigur Street, which has been home to several gay clubs in the last few decades.\u00a0 The mayor of Reykjavik, Dagur B Eggertsson, was among those who painted the street.&quot;,&quot;htmlText&quot;:&quot;<p>Reykjavik Pride has kicked off in Iceland, with events set to take place over the next ten days.<\/p>\n<p>This is the 20th edition of the festival in the country.   <\/p>\n<p>The pre-opening ceremony took place in an unusual location \u2014 the Icelandic Stock Exchange.<\/p>\n<p>Reykjavik Pride\u00b4&amp;#039;s president, Gunnlaugur Bragi Bj\u00f6rnsson, rang the exchange&amp;#039;s bell along with Eva Maria Lange, of the organisation Pink Iceland and Pall Hardarson, president of Nasdaq Iceland, on Thursday morning. <\/p>\n<p>The move was intended to bring into focus the status of LGBTQIA people in the business world.<\/p>\n<p>For the fifth year in a row, a street in Reykjavik was chosen to be painted in the colours of the rainbow flag.<\/p>\n<p>This time, it was Klapparstigur Street, which has been home to several gay clubs in the last few decades. <\/p>\n<p>The mayor of Reykjavik, Dagur B Eggertsson, was among those who painted the street.<\/p>\n&quot;,&quot;hashtag&quot;:null,&quot;createdAt&quot;:1565365864,&quot;publishedAt&quot;:1565369218,&quot;updatedAt&quot;:1565369220,&quot;expiresAt&quot;:0,&quot;canonical&quot;:&quot;https:\/\/www.euronews.com\/2019\/08\/09\/residents-paint-street-in-colours-of-rainbow-flag-to-celebrate-reykjavik-pride&quot;,&quot;images&quot;:[{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/08\/09\/34\/{{w}}x{{h}}_cmsv2_2ea4a9e5-153d-534d-b2e2-c41408653b94-4080934.jpg&quot;,&quot;altText&quot;:null,&quot;caption&quot;:null,&quot;captionUrl&quot;:null,&quot;captionCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;callToActionUrl&quot;:null,&quot;callToActionText&quot;:null,&qu