ands&quot;,&quot;captionCredit&quot;:&quot;Anselmo Pestana Padr\u00f3n&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;callToActionUrl&quot;:null,&quot;callToActionText&quot;:null,&quot;width&quot;:1920,&quot;height&quot;:1080},{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/05\/54\/82\/52\/{{w}}x{{h}}_cmsv2_2dcc81e5-957d-5cc3-9463-d97aa34c4428-5548252.jpg&quot;,&quot;altText&quot;:&quot;Unntove Saetran and Calvin Lucock&quot;,&quot;caption&quot;:&quot;Unntove Saetran and Calvin Lucock&quot;,&quot;captionCredit&quot;:&quot;Founders of Canaria Mama Africa&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;callToActionUrl&quot;:null,&quot;callToActionText&quot;:null,&quot;width&quot;:1920,&quot;height&quot;:1080},{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/05\/54\/82\/52\/{{w}}x{{h}}_cmsv2_93840121-b4f1-5d7e-9296-0a81429bcff9-5548252.jpg&quot;,&quot;altText&quot;:&quot;Ousmane an asylum seeker showing a photo of his daughter he's never met&quot;,&quot;caption&quot;:&quot;Ousmane an asylum seeker showing a photo of his daughter he's never met&quot;,&quot;captionCredit&quot;:&quot;Gran Canaria&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;callToActionUrl&quot;:null,&quot;callToActionText&quot;:null,&quot;width&quot;:1920,&quot;height&quot;:1080},{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/05\/54\/82\/50\/{{w}}x{{h}}_cmsv2_a78b03e2-71f5-5062-b8d6-b70a96d97d0c-5548250.jpg&quot;,&quot;altText&quot;:null,&quot;caption&quot;:null,&quot;captionCredit&quot;:&quot;euronews&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;callToActionUrl&quot;:null,&quot;callToActionText&quot;:null,&quot;width&quot;:1920,&quot;height&quot;:1080}],&quot;authors&quot;:{&quot;journalists&quot;:[],&quot;producers&quot;:[{&quot;