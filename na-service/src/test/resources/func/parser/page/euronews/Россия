u0438\u0437\u043e\u0432\u0430\u043b \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043d\u0443\u044e \u0433\u0440\u0443\u043f\u043f\u0443 \u0432 \u0446\u0435\u043b\u044f\u0445 \u0445\u0438\u0449\u0435\u043d\u0438\u044f \u0431\u044e\u0434\u0436\u0435\u0442\u043d\u044b\u0445 \u0434\u0435\u043d\u0435\u0436\u043d\u044b\u0445 \u0441\u0440\u0435\u0434\u0441\u0442\u0432, \u0432\u044b\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0445 \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043a\u0440\u0435\u0434\u0438\u0442\u0430 \u0434\u043b\u044f \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0438 \u0440\u0435\u0433\u0438\u043e\u043d\u0430. \u042d\u0442\u0430 \u0433\u0440\u0443\u043f\u043f\u0430, \u043a\u0443\u0434\u0430, \u043f\u043e \u0434\u0430\u043d\u043d\u044b\u043c \u0421\u041a, \u0432\u0445\u043e\u0434\u0438\u043b\u0438 \u041c\u0435\u043d\u044c \u0438 \u0431\u044b\u0432\u0448\u0438\u0439 \u043f\u0435\u0440\u0432\u044b\u0439 \u0437\u0430\u043c\u043f\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043b\u044f \u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0418\u0432\u0430\u043d\u043e\u0432\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u041f\u0430\u0432\u0435\u043b \u041a\u043e\u043d\u044c\u043a\u043e\u0432, \u0437\u0430\u043f\u043b\u0430\u043d\u0438\u0440\u043e\u0432\u0430\u043b\u0430 \u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0438\u0442\u044c \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430 \u0438\u0437 \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0431\u044e\u0434\u0436\u0435\u0442\u0430 \u0432 \u0430\u0434\u0440\u0435\u0441 \u041e\u0410\u041e \&quot;\u0418\u0432\u0430\u043d\u043e\u0432\u0441\u043a\u0438\u0439 \u0431\u0440\u043e\u0439\u043b\u0435\u0440\&quot;, \u0432\u0445\u043e\u0434\u044f\u0449\u0435\u0433\u043e \u0432 \&quot;\u041f\u0440\u043e\u0434\u043e\&quot;.  <\/p>\n<p>\u0412\u0438\u043d\u0443 \u041c\u0435\u043d\u044c \u043d\u0435 \u043f\u0440\u0438\u0437\u043d\u0430\u0435\u0442 \u0438 \u0441\u0447\u0438\u0442\u0430\u0435\u0442 \u0441\u0432\u043e\u0435 \u043f\u0440\u0435\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435 \u043d\u0435\u0434\u043e\u0440\u0430\u0437\u0443\u043c\u0435\u043d\u0438\u0435\u043c.<\/p>\n<p>\u041e\u043d \u0431\u044b\u043b \u0437\u0430\u0434\u0435\u0440\u0436\u0430\u043d \u0432 \u0441\u0435\u0440\u0435\u0434\u0438\u043d\u0435 \u043d\u043e\u044f\u0431\u0440\u044f 2020 \u0433\u043e\u0434\u0430. 19 \u043d\u043e\u044f\u0431\u0440\u044f \u0441\u0443\u0434 \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0435\u0433\u043e \u043d\u0430 \u0441\u0432\u043e\u0431\u043e\u0434\u0435 \u043f\u043e\u0434 \u0437\u0430\u043f\u0440\u0435\u0442 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0439: \u0435\u043c\u0443 \u0437\u0430\u043f\u0440\u0435\u0442\u0438\u043b\u0438 \u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f \u0441 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u043c\u0438 \u043b\u044e\u0434\u044c\u043c\u0438, \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u043e\u043c \u0438 \u0441\u0432\u044f\u0437\u044c\u044e \u0438 \u0440\u0430\u0437\u0440\u0435\u0448\u0438\u043b\u0438 \u0432\u044b\u0445\u043e\u0434\u0438\u0442\u044c \u0438\u0437 \u0434\u043e\u043c\u0430 \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430 \u0434\u0432\u0430 \u0447\u0430\u0441\u0430 \u0432 \u0441\u0443\u0442\u043a\u0438. 20 \u0430\u043f\u0440\u0435\u043b\u044f \u0435\u0433\u043e \u043f\u0435\u0440\u0435\u0432\u0435\u043b\u0438 \u043f\u043e\u0434 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0443 \u043e \u043d\u0435\u0432\u044b\u0435\u0437\u0434\u0435.<\/p>\n<p>\u041c\u0438\u0445\u0430\u0438\u043b \u041c\u0435\u043d\u044c \u2014 \u0441\u044b\u043d \u0441\u0432\u044f\u0449\u0435\u043d\u043d\u0438\u043a\u0430 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u0430 \u041c\u0435\u043d\u044f, \u0443\u0431\u0438\u0442\u043e\u0433\u043e \u0432 1990 \u0433\u043e\u0434\u0443 (\u043e\u0431\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0438 \u0432\u0438\u043d\u043e\u0432\u043d\u0438\u043a \u0434\u043e \u0441\u0438\u0445 \u043f\u043e\u0440 \u043d\u0435\u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b). \u0421 2013 \u043f\u043e 2018 \u0433\u043e\u0434 \u041c\u0438\u0445\u0430\u0438\u043b \u041c\u0435\u043d\u044c \u0437\u0430\u043d\u0438\u043c\u0430\u043b \u043f\u043e\u0441\u0442 \u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0430 \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0438 \u0436\u0438\u043b\u0438\u0449\u043d\u043e-\u043a\u043e\u043c\u043c\u0443\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0445\u043e\u0437\u044f\u0439\u0441\u0442\u0432\u0430. \u0414\u043e \u044d\u0442\u043e\u0433\u043e, \u0441 2005 \u043f\u043e 2013, \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u043b \u0418\u0432\u0430\u043d\u043e\u0432\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u044c\u044e. \u0412 2013 \u0433\u043e\u0434\u0443 \u043f\u043e \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u043c\u0443 \u0436\u0435\u043b\u0430\u043d\u0438\u044e \u0443\u0448\u0435\u043b \u0432 \u043e\u0442\u0441\u0442\u0430\u0432\u043a\u0443. \u0421 2018 \u0433\u043e\u0434\u0430 \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u0430\u0443\u0434\u0438\u0442\u043e\u0440\u043e\u043c \u0421\u0447\u0435\u0442\u043d\u043e\u0439 \u043f\u0430\u043b\u0430\u0442\u044b.<\/p>\n&quot;,&quot;hashtag&quot;:null,&quot;createdAt&quot;:1621265507,&quot;publishedAt&quot;:1621268169,&quot;updatedAt&quot;:1621268179,&quot;expiresAt&quot;:0,&quot;canonical&quot;:&quot;https:\/\/ru.euronews.com\/2021\/05\/17\/ru-russian-ex-governor-free&quot;,&quot;images&quot;:[{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/05\/66\/56\/56\/{{w}}x{{h}}_cmsv2_5c04ae22-56e6-5417-a640-76e83dd14ad3-5665656.jpg&