        function init_google_tag(){
            if (dclhandler) { document.removeEventListener('DOMContentLoaded', init_google_tag); }



            renderFirstBanners();

            for (var i = 0 in mobSlots) {
                googletag.cmd.push(function() { googletag.display(mobSlots[i]); });
            }
            $(window).on('resize', function () {
                fix_branding();
            })
        }

