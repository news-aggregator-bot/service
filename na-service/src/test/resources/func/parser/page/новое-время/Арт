        var mobSlots = [];
        var issetSlots = [];
        var rendered_slots = 0;
        function runCatfish(){
            console.log('runCatfish');
            if (iww <= 768) {//mobile
                if(document.getElementById('div-gpt-ad-1564147235034-0-mob') !== null){
                    googletag.cmd.push(function() {
                        googletag.defineSlot('/271925883/new_nv_catfish_mob', [[320,100],[320,50]], 'div-gpt-ad-1564147235034-0-mob')
                            .addService(googletag.pubads())
                        .setTargeting("article", "")
.setTargeting("lang", "ru")
.setTargeting("razdel", "52")
.setTargeting("section", "52")
.setTargeting("url", "https://nv.ua/art.html")
.setTargeting("newnv", "1")
.setTargeting("only_selfpromo", "0");
                        googletag.display('div-gpt-ad-1564147235034-0-mob');
                    });
                }

            }else{
                if(document.getElementById('div-gpt-ad-desktop_catfish') !== null){
                    googletag.cmd.push(function() {
                        googletag.defineSlot('/271925883/catfish_desktop', [[1200, 180]], 'div-gpt-ad-desktop_catfish')
                            .addService(googletag.pubads())
                        .setTargeting("article", "")
.setTargeting("lang", "ru")
.setTargeting("razdel", "52")
.setTargeting("section", "52")
.setTargeting("url", "https://nv.ua/art.html")
.setTargeting("newnv", "1")
.setTargeting("only_selfpromo", "0");
                        googletag.display('div-gpt-ad-desktop_catfish');
                    });
                }

            }


        }
        function renderFirstBanners() {
            console.log('renderFirstBanners');
            googletag.cmd.push(function() {

                for (var i = 0 in dfpSlots) {
                    if (iww >= dfpSlots[i][2][0] && iww < dfpSlots[i][2][1]) {
                        var elemId = i;
                        if (dfpSlots[i][2][0] <= 768 && iww <= 768) {
                            elemId = dfpSlots[i][3] ? i + '-mob' : i;
                            mobSlots.push(elemId);
                        }
                        if (document.getElementById(elemId) !== null && !issetSlots.includes(elemId)) {
                            if(elemId != 'div-gpt-ad-desktop_catfish' && elemId != 'div-gpt-ad-1564147235034-0-mob'){
                                issetSlots.push(elemId);
                                googletag.defineSlot(dfpSlots[i][0], dfpSlots[i][1], ''+elemId+'')
                                    .addService(googletag.pubads())
                                    /*.setCollapseEmptyDiv(true)*/
                                .setTargeting("article", "")
.setTargeting("lang", "ru")
.setTargeting("razdel", "52")
.setTargeting("section", "52")
.setTargeting("url", "https://nv.ua/art.html")
.setTargeting("newnv", "1")
.setTargeting("only_selfpromo", "0");
                             }
                        }else{
                        }
                    }
                }
                googletag.pubads().enableSingleRequest();
                // googletag.pubads().collapseEmptyDivs();
                googletag.enableServices();

                if (document.getElementById("div-gpt-ad-1536827933373-0") !== null) {
                    googletag.display('div-gpt-ad-1536827933373-0');
                }else{
                    googletag.display(issetSlots[0]);
                }

                googletag.pubads().addEventListener('slotRenderEnded', function(e) {
                    var slotElementId = e.slot.getSlotElementId();
                    iframe_for_links = $('#'+slotElementId+" iframe");
                    iframe_for_links.contents().find('a').each(function () {
                        if($(this).attr('target') != '_blank'){
                            $(this).attr('target', '_blank');
                        }
                    });

                                            "reklama"==$("#"+slotElementId).parent().attr("data-type")&&(_parent=$("#"+slotElementId).parent(),console.log("slot - "+slotElementId,e.size),$(_parent).height()<10?$(_parent).hide():(e.size&&e.size[0]&&e.size[1]&&(slot_width=e.size[0],slot_height=e.size[1],console.log(slotElementId,slot_width,slot_height)),_parent.attr("data-type","reklama-rendered"),iframe=$("#"+slotElementId).find("iframe"),iframe.on("load",function(e,t){}),slot_height>350?($(_parent).parents("[data-unit_type]").addClass("big_adv"),$(_parent).parent().css({height:"300px",margin:"5px auto"}),"unit17"===$(_parent).parents("[data-unit_type]").attr("data-unit_type")?($(_parent).parent().prevAll().addClass("big_adv"),$(_parent).parent().nextAll(":lt(2)").addClass("big_adv"),$(_parent).parent().next().next().after('<div class="atom_opinion col-md-3 col-sm-4 col-xs-12" data-atom_type="3"><div class="atom__wrapper atom__wrapper--transparent"></div></div>')):$(_parent).parent().siblings().addClass("big_adv"),$(_parent).parents(".row").find(".atom").last().remove(),$(_parent).parents(".row").find(".atom").last().remove(),$(_parent).parents(".container").find("[data-facebook-promo-for-remove]").first().remove(),$(_parent).parents(".container").find("[data-youtube-promo-for-remove]").css({opacity:0})):($(_parent).parents("[data-unit_type]").addClass("small_adv"),$(_parent).parent().prevAll().addClass("big_adv"),$(_parent).parent().css({height:"300px",margin:"5px auto"}),$(_parent).parents(".row").find(".atom").last().remove(),$(_parent).parents(".container").find("[data-facebook-promo-for-remove]").first().remove())));
                    
                    //Branding
                    if(slotElementId == "div-gpt-ad-1536827933373-0" ) {
                        if(!e.isEmpty){
                            var body = document.getElementsByTagName('body');
                            body[0].classList.add('branding');
                            body[0].classList.remove('fixed-scrolled-header');
