    function showTable(index){
        for(i = 1; i < 7; i++){
            document.getElementById('table'+i).style.display = 'none';
            document.getElementById('table_btn_'+i).classList.remove('active');
        }

        document.getElementById('table_btn_'+index).classList.add('active');

        document.getElementById('table'+index).style.display = 'table';
    }
