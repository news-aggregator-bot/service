        if(lazy_ad === false){
            if(typeof realAgent !== 'undefined' && realAgent == true){
                                    loadScript(vpbSrc, function() { loadScript(gptSrc); });
                            }else{
                loadScript(gptSrc, function () {});
            }
        }

