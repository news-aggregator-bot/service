  <script>
    var js_script_list = [];
    function loadScript(src, cb) {

        var i = js_script_list.length, load_check = false;
        while (i--) {
            if (js_script_list[i] === src) {
                load_check = true;
                break;
            }
        }
        if(!load_check){
            js_script_list.push(src); var s = document.createElement('script'); s.src = src; chead.appendChild(s); s.onload = cb; s.onerror = cb; return s;
        }else{
            if (cb !== undefined && typeof(cb) === "function") {
                cb();
            }
        }
    }
    function loadScriptAmp(src, ce) {var s = document.createElement('script');s.src = src + '?'+cb;s.setAttribute('custom-element', ce);chead.appendChild(s); /*s.onload = cb; s.onerror = cb;*/ return s; }

    
