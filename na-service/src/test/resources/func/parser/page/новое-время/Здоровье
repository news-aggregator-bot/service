        function renderFirstBanners() {
            console.log('renderFirstBanners');
            googletag.cmd.push(function() {

                for (var i = 0 in dfpSlots) {
                    if (iww >= dfpSlots[i][2][0] && iww < dfpSlots[i][2][1]) {
                        var elemId = i;
                        if (dfpSlots[i][2][0] <= 768 && iww <= 768) {
